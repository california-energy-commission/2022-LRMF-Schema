<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/LMCVPLB21H" version="2022.0.000" xmlns="http://www.lmonte.com/besm/LMCVPLB21H" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCVPLB21H"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF VERIFICATION"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="HERS Verified Multifamily Central Hot Water System Distribution"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderHERS"/>
                     <xsd:element ref="lMCVPLB21H"/>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor"/>
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF3R"/>
                     <xsd:element name="Footer" type="comp:Footer"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCVPLB21H"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCVPLB21H">
      <xsd:annotation>
         <xsd:documentation>HERS Verified Multifamily Central Hot Water System Distribution</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">General Information</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A01_BuildingName" type="com:BuildingName">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Name</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Design HERS Verified Central Water Heating Systems Information (other than CHPWH)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This  section is required if the LMCC parent  has any non-HPWH systems, Require one row of data for each non-HPWH system identified on the LMCC</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports features of he water heating system other than CHPWH system that were specified on the registered LMCC  compliance document for this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDesignSystem">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="B01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from LMCC.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B02_ResidentialWaterHeatingSystemType" type="hvac:ResidentialWaterHeatingSystemType">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heating System Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance, reference values from LMCC-PRF; Allowed values are: DHW display term DHW,  Hydronic display term Hydronic and  HydronicSpaceConditioningDHW display term Combined Hydronic; Else If Prescriptive result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B02_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heating System Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance, reference values from LMCC-PRF; Allowed values are: DHW display term DHW,  Hydronic display term Hydronic and  HydronicSpaceConditioningDHW display term Combined Hydronic; Else If Prescriptive result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B03_ResidentialWaterHeaterType" type="hvac:ResidentialWaterHeaterType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water-Heater Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from LMCC Allowed values are: BoilerWaterHeater display term: Boiler,   IndirectWaterHeating display term:  Indirect, InstantaneousCommercial display term: Commercial Instantaneous, InstantaneousResDutyCommercial display term: Residential-Duty Commercial Instantaneous, InstantaneousConsumer display term: Consumer Instantaneous,   StorageCommercial display term: Commercial Storage, StorageResDutyCommercial display term: Residential-Duty Commercial Storage,   StorageConsumer display term: Consumer Storage.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B04_WaterHeaterTotalSystemCount" type="hvac:WaterHeaterTotalSystemCount">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Number of Water Heaters in System</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from LMCC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B05_StorageCapacity" type="hvac:StorageCapacity">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Storage Volume (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC; N/A is allowed only if B03_ResidentialWaterHeaterType == InstantaneousCommercial, InstantaneousResDutyCommercial Or InstantaneousConsumer</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B05_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Storage Volume (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC; N/A is allowed only if B03_ResidentialWaterHeaterType == InstantaneousCommercial, InstantaneousResDutyCommercial Or InstantaneousConsumer</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B06_WaterHeaterFuelSource">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fuel Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from LMCC Allowed values are: Electricity; NaturalGas; PropaneGas, WaterHeatingHP</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:WaterHeaterFuelSource">
                                             <xsd:enumeration value="Electricity"/>
                                             <xsd:enumeration value="NaturalGas"/>
                                             <xsd:enumeration value="PropaneGas"/>
                                             <xsd:enumeration value="WaterHeatingHP"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B07_WaterHeaterRatedInputUnits" type="hvac:WaterHeaterRatedInputUnits">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B06_WaterHeaterFuelSource == WaterHeatingHP then result = N/A stored in the NotApplicableMessage; Else If B06_WaterHeaterFuelSource == NaturalGas; PropaneGas result = BtuHr display term: Btu/hr,  Else If B06_WaterHeaterFuelSource == Electricity result = kW display term: kW</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B06_WaterHeaterFuelSource == WaterHeatingHP then result = N/A stored in the NotApplicableMessage; Else If B06_WaterHeaterFuelSource == NaturalGas; PropaneGas result = BtuHr display term: Btu/hr,  Else If B06_WaterHeaterFuelSource == Electricity result = kW display term: kW</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B08_WaterHeaterElectricFiredRatedInput" type="hvac:WaterHeaterElectricFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_ResidentialWaterHeaterType == HeatPumpWaterHeater then result = N/A stored in NotApplicableMessage; Else If Performance  reference value from LMCC-PRF  for electric fired or gas fired water heater; Else If Prescriptive result = N/A stored in NotApplicableMessage.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B08_WaterHeaterGasFiredRatedInput" type="hvac:WaterHeaterGasFiredRatedInput">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_ResidentialWaterHeaterType == HeatPumpWaterHeater then result = N/A stored in NotApplicableMessage; Else If Performance  reference value from LMCC-PRF  for electric fired or gas fired water heater; Else If Prescriptive result = N/A stored in NotApplicableMessage.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B08_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Input Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_ResidentialWaterHeaterType == HeatPumpWaterHeater then result = N/A stored in NotApplicableMessage; Else If Performance  reference value from LMCC-PRF  for electric fired or gas fired water heater; Else If Prescriptive result = N/A stored in NotApplicableMessage.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B09_EfficiencyType">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance reference values from LMCC-PRF for efficiency type  Allowed values are: AFUE, ThermalEfficiency, Uniform Energy Factor; Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:EfficiencyType">
                                                <xsd:enumeration value="AFUE"/>
                                                <xsd:enumeration value="ThermalEfficiency"/>
                                                <xsd:enumeration value="UniformEnergyFactor"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="B09_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance reference values from LMCC-PRF for efficiency type  Allowed values are: AFUE, EnergyFactor, ThermalEfficiency, Uniform Energy Factor; Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B10_WaterHeaterAFUE" type="hvac:WaterHeaterAFUE">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance reference values from LMCC-PRF Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B10_EnergyFactor" type="hvac:EnergyFactor">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance reference values from LMCC-PRF Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B10_UniformEnergyFactor" type="hvac:UniformEnergyFactor">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance reference values from LMCC-PRF Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B10_ThermalEfficiency" type="hvac:ThermalEfficiency">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance reference values from LMCC-PRF Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B10_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Performance reference values from LMCC-PRF Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B11_WaterHeatingStandbyLossPercent" type="hvac:WaterHeatingStandbyLossPercent">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Standby Loss (%)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If  Performance reference value from LMCC-PRF-01 Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B11_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Standby Loss (%)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If  Performance reference value from LMCC-PRF-01 Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B12_ExteriorTankInsulation" type="hvac:ExteriorTankInsulation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Insulation R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If  Performance reference value from LMCC-PRF-01 Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B12_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Insulation R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If  Performance reference value from LMCC-PRF-01 Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B2" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Design HERS Verified CHPWH System Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if the registered LMCC has at least one system that is a CHPWH system.  Require one row of data for each water heater identified on the LMCC</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports the water heating systems specified on the registered LMCC compliance document for this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableHpwhd">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Hpd01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from LMCC.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd02_HeatPumpWaterHeaterBrand" type="hvac:HeatPumpWaterHeaterBrand">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Modeled Equipment Make and Model</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC-PRF-01</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd02_HeatPumpWaterHeaterModel" type="hvac:HeatPumpWaterHeaterModel">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Modeled Equipment Make and Model</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance LMCC reference value from LMCC-PRF and store in HeatPumpWaterHeaterModel
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Hpd03_CompressorCountCHPWH" type="hvac:CompressorCountCHPWH">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of Water Heaters/ Compressors</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from LMCC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hpd04_TankLocation" type="hvac:TankLocation">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Primary Tank Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from LMCC.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd05_StorageCapacity" type="hvac:StorageCapacity">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Tank Volume</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC which may be NA.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd05_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Tank Volume</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC which may be NA.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd06_PrimaryTankTotalInsulation" type="hvac:PrimaryTankTotalInsulation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Tank Total Insulation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC which may be NA.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd06_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Tank Total Insulation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC which may be NA.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd07_TankLocation" type="hvac:TankLocation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC which may be NA</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd08_LoopTankCapacity" type="hvac:LoopTankCapacity">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Volume</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC which may be NA.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd08_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Volume</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd09_LoopTankTotalInsulation" type="hvac:LoopTankTotalInsulation">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Total Insulation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC which may be NA.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd09_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Total Insulation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd10_PipeInsulationThickness" type="hvac:PipeInsulationThickness">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Pipe Insulation Thickness</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC which may be NA.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd10_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Pipe Insulation Thickness</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpd11_LoopTankType" type="hvac:LoopTankType">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from LMCC</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpd11_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
