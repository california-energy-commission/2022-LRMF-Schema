                                          <xsd:restriction base="hvac:ResidentialDHWCentralDistributionType">
                                             <xsd:enumeration value="NoRecirculationMF"/>
                                             <xsd:enumeration value="RecirculatingDemandControlMF"/>
                                             <xsd:enumeration value="RecirculationMF"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
