<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/LMCVEXC20H" version="2022.0.000" xmlns="http://www.lmonte.com/besm/LMCVEXC20H" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCVEXC20H"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF VERIFICATION"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="Existing Conditions For Residential Alterations"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderCF2R_3R"/>
                     <xsd:element ref="lMCVEXC20H"/>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor"/>
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF3R"/>
                     <xsd:element name="Footer" type="comp:Footer"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCVEXC20H"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCVEXC20H">
      <xsd:annotation>
         <xsd:documentation>Existing Conditions For Residential Alterations</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">General Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A01_ProjectName" type="com:ProjectName">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Name</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_RunDescription" type="comp:RunDescription">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Calculation Description</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_StreetAddress" type="com:StreetAddress">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_City" type="com:City">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">CA City</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_Title24StandardsVersion" type="comp:Title24StandardsVersion">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Standards Version</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_Zipcode" type="dtyp:Zipcode">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip code</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_ComplianceSoftwareVersion" type="comp:ComplianceSoftwareVersion">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Software Version</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_ClimateZone" type="com:ClimateZone">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A09_Azimuth" type="dtyp:Azimuth">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Front Orientation (deg/Cardinal)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A09_CardinalOrientation" type="com:CardinalOrientation">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Front Orientation (deg/Cardinal)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="A10_BuildingTotalVolume" type="com:BuildingTotalVolume">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Building Volume (ft<d:sup>3</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A11_DwellingUnitCount" type="com:DwellingUnitCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Dwelling Units</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A12_ProjectScopePerformance" type="comp:ProjectScopePerformance">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Scope</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A13_BedroomCount" type="com:BedroomCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Bedrooms</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A14_NewConditionedFloorArea" type="bld:NewConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">New Conditioned Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A15_TotalStoryCount" type="com:TotalStoryCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Stories in Building</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A16_ExistingConditionedFloorArea" type="bld:ExistingConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Existing Conditioned Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A17_UFactorFenestrationAverage" type="com:UFactorFenestrationAverage">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fenestration Average U-factor</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A18_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Conditioned Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A19_FenestrationFloorRatio" type="env:FenestrationFloorRatio">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Glazing Percentage (%)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Opaque Surfaces</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If existing data does not appear in LMCC, then display the Section Does Not Apply message
 
In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableOpaqueSurfaces">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="B01_PartitionName" type="env:PartitionName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B02_ThermalZoneName" type="hvac:ThermalZoneName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Zone</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B03_ConstructionAssemblyNamePerformanceRes" type="comp:ConstructionAssemblyNamePerformanceRes">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Existing Conditions</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B04_OpaqueSurfaceType" type="env:OpaqueSurfaceType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Surface Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B05_Azimuth" type="dtyp:Azimuth">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Azimuth</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B05_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Azimuth</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B06_RelativePosition" type="com:RelativePosition">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Orientation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC;  Value may be one of the following: relative positions of front, back, left or right, or the degrees arc relative to the front being zero if it doesn't align with one of the four relative positions; or the  value may be N/A if the surface is a floor or roof.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B06_RelativePositionDegrees" type="com:RelativePositionDegrees">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Orientation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC;  Value may be one of the following: relative positions of front, back, left or right, or the degrees arc relative to the front being zero if it doesn't align with one of the four relative positions; or the  value may be N/A if the surface is a floor or roof.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B06_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Orientation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC;  Value may be one of the following: relative positions of front, back, left or right, or the degrees arc relative to the front being zero if it doesn't align with one of the four relative positions; or the  value may be N/A if the surface is a floor or roof.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B07_InsulationRValue" type="env:InsulationRValue">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Total Cavity R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Total Cavity R-value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B08_ExistingConditionVerificationStatus" type="comp:ExistingConditionVerificationStatus">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Verification</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If this existing condition has been verified, user selects Pass (Boolean true), Else if any part of this existing condition is not verified, user selects Fail (Boolean false).</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="B09_DoAllExistingConditionsComply" type="comp:DoAllExistingConditionsComply">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Verification Status</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If the value for ExistingConditionVerificationStatus in all rows of this section equals true, then result is true.  Else If any values for ExistingConditionVerificationStatus equal false then result is false.  For Boolean true value, display text: Pass - all existing conditions have been verified.  For Boolean false value, display text:   Fail - one or more of the existing conditions not present. Enter reason for failure in correction notes field below</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B10_SectionCorrectionNotes" type="comp:SectionCorrectionNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Correction Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Roofs</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If existing data does not appear in LMCC, then display the Section Does Not Apply message
 
In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableAttic">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="C01_PartitionName" type="env:PartitionName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C02_ConstructionAssemblyNamePerformanceRes" type="comp:ConstructionAssemblyNamePerformanceRes">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Construction</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C03_RoofRise" type="env:RoofRise">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Rise</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C04_SolarReflectanceAged" type="com:SolarReflectanceAged">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Reflectance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C05_ThermalEmittance" type="com:ThermalEmittance">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Emittance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C06_RadiantBarrierToBeInstalled" type="comp:RadiantBarrierToBeInstalled">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Radiant Barrier</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C07_CoolRoofToBeInstalled" type="comp:CoolRoofToBeInstalled">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Cool Roof</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C08_ExistingConditionVerificationStatus" type="comp:ExistingConditionVerificationStatus">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Verification</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If this existing condition has been verified, user selects Pass (Boolean true), Else if any part of this existing condition is not verified, user selects Fail (Boolean false).</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="C09_DoAllExistingConditionsComply" type="comp:DoAllExistingConditionsComply">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Verification Status</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If the value for ExistingConditionVerificationStatus in all rows of this section equals true, then result is true.  Else If any values for ExistingConditionVerificationStatus equal false then result is false.  For Boolean true value, display text: Pass - all existing conditions have been verified.  For Boolean false value, display text:   Fail - one or more of the existing conditions not present. Enter reason for failure in correction notes field below</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C10_SectionCorrectionNotes" type="comp:SectionCorrectionNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Correction Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Windows</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If existing data does not appear in LMCC, then display the Section Does Not Apply message
In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableWindows">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="D01_FenestrationVerticalName" type="env:FenestrationVerticalName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered  parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D02_Azimuth" type="dtyp:Azimuth">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Azimuth</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered  parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D03_TypeMultiplier" type="comp:TypeMultiplier">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Multiplier</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered  parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D04_GlazingArea" type="env:GlazingArea">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered  parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D05_UFactor" type="com:UFactor">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">U-factor</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered  parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D06_SHGC" type="com:SHGC">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SHGC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Data is loaded from registered  parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D07_ExternalShadingObjectType" type="env:ExternalShadingObjectType">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Shading</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC; allow N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Shading</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Data is loaded from registered parent CC; allow N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="D08_ExistingConditionVerificationStatus" type="comp:ExistingConditionVerificationStatus">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Verification</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If this existing condition has been verified, user selects Pass (Boolean true), Else if any part of this existing condition is not verified, user selects Fail (Boolean false).</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
