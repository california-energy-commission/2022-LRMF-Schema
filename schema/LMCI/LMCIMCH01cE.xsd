<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/LMCIMCH01cE"
   xmlns="http://www.lmonte.com/besm/LMCIMCH01cE"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2022.0.000">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCIMCH01cE"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF INSTALLATION"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="Space Conditioning Systems, Ducts, and Fans"/>
                  <xsd:attribute name="docVariantSubtitle" type="comp:ComplianceDocumentVariantSubtitle" fixed="MCH01c Prescriptive Newly Constructed Buildings"/>
                  <xsd:attribute name="docVariantLetter" type="comp:ComplianceDocumentVariant" fixed="c"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderHERS" />
                     <xsd:element ref="lMCIMCH01cE" />
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF2R_H" />
                     <xsd:element name="Footer" type="comp:Footer" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCIMCH01cE"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCIMCH01cE">
      <xsd:annotation>
         <xsd:documentation>Space Conditioning Systems, Ducts, and Fans - MCH01c Prescriptive Newly Constructed Buildings</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">General Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A01_DwellingUnitName" type="com:DwellingUnitName" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Dwelling Unit Name</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Reference text from parent CC. if available, else use the data in the Project Name field</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ClimateZone" type="com:ClimateZone" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Reference text from parent CC</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_DwellingUnitConditionedFloorArea" type="com:DwellingUnitConditionedFloorArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Dwelling Unit Total Conditioned Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Value is numeric: xxxxx  
If  parent CC is LMCC-PRF, then 
If project scope = Newly Constructed (Addition Alone) 
prompt user to enter a value equal to dwelling unit existing CFA + addition CFA, 
Else reference the value from LMCC-PRF And  Aalow user to override default and input value; flag overridden values and report in project status notes field
End If  
Else If  parent CC is LMCC-MCH, then
User enters value that is equal to dwelling unit CFA      
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_ResidentialDwellingUnitSpaceConditioningCount" type="hvac:ResidentialDwellingUnitSpaceConditioningCount" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Space Conditioning Systems in this Dwelling Unit.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input the integer value xx</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_ComplianceMethod" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Certificate of Compliance Type</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Reference document type property from LMCC; For choice Performance display term performance (LMCC-PRF); for choice PrescriptiveLMCC  display term: Prescriptive  (LMCC-MCH)</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="Performance">Performance (LMCC-PRF)</dtyp:displayterm>
                           <dtyp:displayterm value="PrescriptiveLMCC">Prescriptive  (LMCC-MCH)</dtyp:displayterm>
                        </xsd:appinfo>
                        
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:ComplianceMethod">
                           <xsd:enumeration value="Performance"/>
                           <xsd:enumeration value="PrescriptiveLMCC"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A06_HvacLoadCalculationMethod" type="comp:HvacLoadCalculationMethod" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Method Used to Calculate HVAC Loads (See Section 160.3(b)1)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input;  Choices to display: ASHRAE Handbook;  SMACNA Residential Comfort System Installation Standards Manual, ACCA Manual J, Not applicable - equipment changeout, like-for-like.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A07_DesignSensibleCoolingLoad" type="hvac:DesignSensibleCoolingLoad" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Calculated Dwelling Unit Sensible Cooling Load (Btu/h)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If   A06_HvacLoadCalculationMethod == NotApplicableEquipmentChangeout Then result = N/A stored in the  NotAppplicableMessage Else User entry integer xxxxx</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Calculated Dwelling Unit Sensible Cooling Load (Btu/h)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If   A06_HvacLoadCalculationMethod == NotApplicableEquipmentChangeout Then result = N/A stored in the  NotAppplicableMessage Else User entry integer xxxxx</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="A08_DwellingUnitDesignHeatingLoad" type="hvac:DwellingUnitDesignHeatingLoad" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Calculated Dwelling Unit Heating Load (Btu/h)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If   A06_HvacLoadCalculationMethod == NotApplicableEquipmentChangeout Then result = N/A stored in the  NotAppplicableMessage Else User entry integer xxxxx</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Calculated Dwelling Unit Heating Load (Btu/h)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If   A06_HvacLoadCalculationMethod == NotApplicableEquipmentChangeout Then result = N/A stored in the  NotAppplicableMessage Else User entry integer xxxxx</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="A09_BedroomCount" type="com:BedroomCount" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Dwelling Unit Number of Bedrooms</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field: integer xx;  If A05_ComplianceMethod == Performance Then use as default the value referenced from the parent CC Or allow user to override  default and input a new value, constrained to be GTE to default value from parent CC, flag non-default values and report of change in the project status notes field.  
Else If ComplianceMethod != Performance  then user inputs integer value.
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A09After" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Variant MCH-01c</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">MCH-01c - Space Conditioning Systems Ducts and Fans - Prescriptive, Newly Constructed Buildings</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field: 
If A05_ComplianceMethod  == Performance, 
If LMCC-PRF project scope == Addition and/or Alteration Or Newly Constructed (Addition Alone) then display doc variation MCH-01d ;  
Else If  LMCC-PRF project scope == Newly Constructed display doc variation  MCH-01a 
End If  
 
Else If ComplianceMethod == PrescriptiveLMCC  then
If LMCC-MCH ProjectScopeNR ==  ExistingAdditionAndAlteration Or ExistingAlteration 
display doc variation MCH-01b;  
Else If ProjectScopeNR == NewComplete Or AdditionComplete 
display doc variation MCH-01c  
End If
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Design Space Conditioning (SC) System Component Specifications from CF1R</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)
 
Require one row of data in this table for each of the SC Systems identified on the parent CC report that is applicable to this dwelling unit.  Do not allow user to overwrite these referenced data</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">This table reports the space conditioning system features that were specified on the registered LMCC compliance document for this project.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDesignComponents" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="B01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/Name from  parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto filled text: referenced from  parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B02_ResidentialHeatingSystemType" type="hvac:ResidentialHeatingSystemType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Heating System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto filled text: referenced from  parent CC</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B03_EfficiencyType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B02_ResidentialHeatingSystemType == Hydronic, CombinedHydronic, HydronicForcedAir, CombinedHydronicForcedAir, HydronicHP,  Or HydronicHP_ForcedAir   result = NotApplicable stored in B03_NotApplicableMessage Else auto fill value referenced from parent CC for this system name</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:EfficiencyType">
                                                <xsd:enumeration value="AFUE"/>
                                                <xsd:enumeration value="COP"/>
                                                <xsd:enumeration value="HSPF"/>
                                                <xsd:enumeration value="HSPF2"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="B03_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B02_ResidentialHeatingSystemType == Hydronic, CombinedHydronic, HydronicForcedAir, CombinedHydronicForcedAir, HydronicHP,  Or HydronicHP_ForcedAir   result = NotApplicable stored in B03_NotApplicableMessage Else auto fill value referenced from parent CC for this system name</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B04_EfficiencyMinimumValueAFUE" type="hvac:EfficiencyMinimumValueAFUE" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplicableMessage IsPresent   
result = NotApplicable stored in B04_NotApplicableMessage
Else  auto fill AFUE value referenced from parent CC for this system name</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B04_EfficiencyMinimumValueCOP" type="hvac:EfficiencyMinimumValueCOP" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplicableMessage IsPresent   
result = NotApplicable stored in B04_NotApplicableMessage
 Else  auto fill COP value referenced from parent CC for this system name</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B04_EfficiencyMinimumValueHSPF" type="hvac:EfficiencyMinimumValueHSPF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplicableMessage IsPresent   
result = NotApplicable stored in B04_NotApplicableMessage
 Else  auto fill HSPF value referenced from parent CC for this system name</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B04_EfficiencyMinimumValueHSPF2" type="hvac:EfficiencyMinimumValueHSPF2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplicableMessage IsPresent   
result = NotApplicable stored in B04_NotApplicableMessage
Else  auto fill HSPF2 value referenced from parent CC for this system name</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplicableMessage IsPresent   
result = NotApplicable stored in B04_NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B05_ResidentialCoolingSystemType" type="hvac:ResidentialCoolingSystemType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Cooling System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B06_EfficiencyType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced fromparent CC for this system name   Else If data is not available on the parent CC Or B05_ResidentialCoolingSysstemType == NoCooling result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:EfficiencyType">
                                                <xsd:enumeration value="CEER"/>
                                                <xsd:enumeration value="EER"/>
                                                <xsd:enumeration value="EER2"/>
                                                <xsd:enumeration value="EER2SEER2"/>
                                                <xsd:enumeration value="EERSEER"/>
                                                <xsd:enumeration value="SEER"/>
                                                <xsd:enumeration value="SEER2"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="B06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name   Else If data is not available on the parent CC Or B05_ResidentialCoolingSysstemType == NoCooling result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B07_EfficiencyMinimumValueSEER" type="hvac:EfficiencyMinimumValueSEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value SEER/SEER2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name If B06_EfficiencyType ==SEER or EERSEER store result in EfficiencyMinimumValueSEER   Else  If data is not available on the parent CC Or B05_ResidentialCoolingSystemType == NoCooling result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07_EfficiencyMinimumValueSEER2" type="hvac:EfficiencyMinimumValueSEER2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value SEER/SEER2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name 
If B06_EfficiencyType ==SEER2  or EER2SEER2 store result in EfficiencyMinimumValueSEER2   Else  If data is not available on the parent CC Or B05_ResidentialCoolingSystemType == NoCooling result = N/A stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value SEER/SEER2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If data is not available on the parent CC Or B05_ResidentialCoolingSystemType == NoCooling result = N/A stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B07b_EfficiencyMinimumValueEER" type="hvac:EfficiencyMinimumValueEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name If B06_EfficiencyType ==EER  or EERSEER store result in EfficiencyMinimumValueEER   Else  If data is not available on the parent CC Or B05_ResidentialCoolingSystemType == NoCooling result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07b_EfficiencyMinimumValueEER2" type="hvac:EfficiencyMinimumValueEER2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name If B06_EfficiencyType ==EER2  or EER2SEER2 store result in EfficiencyMinimumValueEER2   Else  If data is not available on the parent CC Or B05_ResidentialCoolingSystemType == NoCooling result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07b_EfficiencyMinimumValueCEER" type="hvac:EfficiencyMinimumValueCEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name If B06_EfficiencyType == CEER store result in EfficiencyMinimumValueCEER   Else  If data is not available on the parent CC Or B05_ResidentialCoolingSystemType == NoCooling result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07b_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name   Else If data is not available on the parent CC Or B05_ResidentialCoolingSystemType == NoCooling result = N/A stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B08_DuctSystemType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Distribution System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC  for this system name</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:DuctSystemType">
                                             <xsd:enumeration value="DuctedSystem"/>
                                             <xsd:enumeration value="Ductless"/>
                                             <xsd:enumeration value="Piping"/>
                                             <xsd:enumeration value="Radiant"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="B09_DuctLocation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Duct Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:DuctLocation">
                                             <xsd:enumeration value="ConditionedSpaceExcept12ft"/>
                                             <xsd:enumeration value="DuctedAndDuctlessIndoorUnits"/>
                                             <xsd:enumeration value="Ductless"/>
                                             <xsd:enumeration value="DuctsInMultipleLocations"/>
                                             <xsd:enumeration value="Outdoors"/>
                                             <xsd:enumeration value="UnconditionedAttic"/>
                                             <xsd:enumeration value="UnconditionedCrawlSpace"/>
                                             <xsd:enumeration value="UnconditionedGarage"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B10_DuctRValue" type="hvac:DuctRValue" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Duct R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill value: referenced from parent CC for this system name if data is available; Else if parent CC does not provide R-values for this system, result == N/A stored in B10_notApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Duct R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill value: referenced from parent CC for this system name if data is available; Else if parent CC does not provide R-values for this system, result == N/A stored in B10_notApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B11_ThermostatType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Thermostat Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name; Allow N/A</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:ThermostatType">
                                                <xsd:enumeration value="EnergyManagementSystem"/>
                                                <xsd:enumeration value="OccupantControlledSmartThermostat"/>
                                                <xsd:enumeration value="SetbackThermostat"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="B11_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Thermostat Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name; Allow N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B12_ComplianceDocumentTableComments" type="comp:ComplianceDocumentTableComments" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Comments</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill text referenced from parent CC for this system name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Space Conditioning (SC) System Component Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)
 
Require one row of data to be entered in this table for each space conditioning system in the count field A04, ResidentialDwellingUnitSpaceConditioningCount</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableComponents" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="C01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/ Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User select applicable system name from a list comprised of the systems identified in section B, field B01.  If SC system names from parent CC are installed more than once in this dwelling unit, then duplicate SC System names are allowed in this field.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input text 15 characters maximum.  Require each entry to be unique in this dwelling unit, i.e. unique within the scope of this instance of the MCH-01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C03_ResidentialHvacSystemConditionedArea" type="hvac:ResidentialHvacSystemConditionedArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Conditioned Floor Area Served by the System (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric xxxx; Require the sum of the values in this column to be equal to the value in A03 as condition of completion of the document.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C04_ResidentialHeatingSystemType" type="hvac:ResidentialHeatingSystemType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Heating System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B02 as default;   allow user to override the default and select one from choices for ResidentialHeatingSystemType:   CentralGasFurnace, CentralSplitHP, CentralPackagedHP, CentralLargePackagedHP, DuctlessSplitHP (display term Ductless mini-split HP), RoomHP, Boiler, Hydronic, CombinedHydronic, HydronicForcedAir, CombinedHydronicForcedAir, HydronicHP,  HydronicHP_ForcedAir, GasPackagedFurnace, GasWallFurnace , GasSpaceHeater, Electric, NonAirSourceHP, WoodHeat, SmallDuctHighVelocityHP, DuctlessMultiSplitHP, DuctedDuctlessMultiSplitHP, DuctlessVRF_HP, DuctedMiniSplitHP, VCHP_IndoorUnitDucted, VCHP_IndoorUnitDuctless,  VCHP_IndoorUnitDuctedAndDuctless, SPVHP, PTHP and NoHeating.  If result == NoHeating check that there is at least one heating system entered in this section in C04 to comply; Else flag non-compliant condition (no heating installed) and do not allow registration to continue.     Flag non-default values and report in project status notes field; a revised parent CC may be required.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C05_ResidentialCoolingSystemType" type="hvac:ResidentialCoolingSystemType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Cooling System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B05 as default;   allow user to override the default and select one from choices for ResidentialCoolingSystemType:  CentralLargePackagedAC, CentralLargePackagedHP, CentralPackagedAC, CentralPackagedHP, CentralSplitAC, CentralSplitHP, DuctedMultiSplitAC, DuctlessMultiSplitAC, DuctedDuctlessMultiSplitAC, DuctedMultiSplitHP, DuctlessMultiSplitHP, DuctedDuctlessMultiSplitHP, DuctlessSplitAC (display term Ductless mini-split AC) , DuctlessSplitHP (display term Ductless mini-split HP), DuctlessVRF_AC, DuctlessVRF_HP, EvaporativeDirect, EvaporativeIndirect, EvaporativeIndirectDirect, EvaporativelyCooledCondenser, GasAbsorptionAC, GroundSourceHeatPump, Hydronic, HydronicHP, HydronicHP_ForcedAir, IceStorageAC, NoCooling, NonAirCooledAC, NonAirSourceHP, RoomAC, RoomHP, SmallDuctHighVelocityHP, SmallDuctHighVelocityAC.  DuctedMiniSplitAC, DuctedMiniSplitHP, VCHP_IndoorUnitDucted, VCHP_IndoorUnitDuctless,  VCHP_IndoorUnitDuctedAndDuctless SPVAC, SPVHP, PTAC, PTHP;  Note: NoCooling value indicates a heating-only system.  Flag non-default values and report in project status notes field; a revised parent CC may be required.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C06_DuctSystemType" type="hvac:DuctSystemType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Distribution System Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B08 DuctSystemType; Allow user to override and pick from list. DuctedSystem display term Ducted System, Ductless display term System has no ducts, DuctedAndDuctlessIndoorUnits display term Multiple split Indoor Units combined Ducted and Ductless</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C07_DuctLocation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Duct Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value for DuctLocation from B09 as default;   Allow user to override and pick from list.  Choices for DuctLocation and display text:   ConditionedSpace display text: HVAC system(s) with all HVAC ducts located in conditioned space.    ConditionedSpaceExcept12ft display text: Ducts located within the conditioned space (except less than 12 lineal ft).    DuctsInMultipleLocations display text: Ducts located in multiple places.   Outdoors display text: Ducts located in exposed outdoor locations.  UnconditionedAttic display text: Ducts located overhead in unconditioned attic.  UnconditionedCrawlSpace display text: Ducts located underfloor in unconditioned crawl space.  UnconditionedGarage display text: Ducts located in an unconditioned garage.  Flag non-default values and report in project status notes field; a revised parent CC may be required.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:DuctLocation">
                                             <xsd:enumeration value="ConditionedSpace"/>
                                             <xsd:enumeration value="ConditionedSpaceExcept12ft"/>
                                             <xsd:enumeration value="DuctsInMultipleLocations"/>
                                             <xsd:enumeration value="Outdoors"/>
                                             <xsd:enumeration value="UnconditionedAttic"/>
                                             <xsd:enumeration value="UnconditionedCrawlSpace"/>
                                             <xsd:enumeration value="UnconditionedGarage"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="C08_ThermostatType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SC System Thermostat Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from B11 as default;   allow user to override the default and select one from list. OccupantControlledSmartThermostat display term OCST per JA5, SetbackThermostat display term Setback, EnergyManagementSystem display term Energy Management System (EMS); Allow N/A value If B11 is N/A</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:ThermostatType">
                                                <xsd:enumeration value="EnergyManagementSystem"/>
                                                <xsd:enumeration value="OccupantControlledSmartThermostat"/>
                                                <xsd:enumeration value="SetbackThermostat"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="C08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SC System Thermostat Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from B11 as default;   allow user to override the default and select one from list. OccupantControlledSmartThermostat display term OCST per JA5, SetbackThermostat display term Setback, EnergyManagementSystem display term Energy Management System (EMS); Allow N/A value If B11 is N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="C09_ResidentialCoolingZoneType" type="hvac:ResidentialCoolingZoneType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Zoning Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C05_CoolingSystemType ==  NoCooling, then result = NotApplicable stored in the NotApplicableMessage and display text:  N/A;  Else allow user to pick from list.     Choices are ZoneControlled,  display term Zonally Controlled;  NotZoneControlled, display term NotZonal</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="C09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Zoning Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C05_CoolingSystemType ==  NoCooling, then result = NotApplicable stored in the NotApplicableMessage and display text:  N/A;  Else allow user to pick from list.     Choices are ZoneControlled,  display term Zonally Controlled;  NotZoneControlled, display term NotZonal</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="C10_CondenserSpeedType" type="hvac:CondenserSpeedType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling System Compressor Speed Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C05_CoolingSystemType ==  NoCooling, then result =  NotApplicable stored in the NotApplicableMessage and display text:  N/A;  Else  user picks from list.  Choices for CompressorSpeedType: MultiSpeed display term Multi-speed,   SingleSpeed display term Single Speed.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="C10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling System Compressor Speed Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C05_CoolingSystemType ==  NoCooling, then result =  NotApplicable stored in the NotApplicableMessage and display text:  N/A;  Else  user picks from list.  Choices for CompressorSpeedType: MultiSpeed display term Multi-speed,   SingleSpeed display term Single Speed.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="C11_IndoorUnitsCount" type="hvac:IndoorUnitsCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Number of Indoor Units for this System</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C04 Or C05 ==  RoomAC, RoomHP, GasWallFurnace , GasSpaceHeater, Electric, WoodHeat, GasPackagedFurnace, CentralPackagedAC, CentralPackagedHP, CentralLargePackagedAC, CentralLargePackagedHP, EvaporativeDirect, EvaporativeIndirect, EvaporativeIndirectDirect, EvaporativelyCooledCondenser result = N/A stored in NotApplicableMessage; 
 
Else If parent CC requires CFI IAQ Ventilation system, result = 1, Else result = 1 as default  but allow user to overwrite with integer GT 1 Or N/A stored as NotApplicable in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="C11_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Number of Indoor Units for this System</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C04 Or C05 ==  RoomAC, RoomHP, GasWallFurnace , GasSpaceHeater, Electric, WoodHeat, GasPackagedFurnace, CentralPackagedAC, CentralPackagedHP, CentralLargePackagedAC, CentralLargePackagedHP, EvaporativeDirect, EvaporativeIndirect, EvaporativeIndirectDirect, EvaporativelyCooledCondenser result = N/A stored in NotApplicableMessage; 
 
Else If parent CC requires CFI IAQ Ventilation system, result = 1, Else result = 1 as default  but allow user to overwrite with integer GT 1 Or N/A stored as NotApplicable in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="C11a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Heating Equipment Information (not heat pumps)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)
 
This Section is applicable only if at least one row in C04_ResidentialHeatingSystemType != one of these Heat Pump types: CentralLargePackagedHP, CentralPackagedHP, CentralSplitHP,  DuctlessSplitHP (display term Ductless mini-split HP),  DuctedMultiSplitHP, DuctlessMultiSplitHP, DuctedDuctlessMultiSplitHP, DuctlessVRF_HP, SmallDuctHighVelocityHP, HydronicHP, HydronicHP_ForcedAir, RoomHP, DuctedMiniSplitHP, AirToWaterHeatPump, GroundSourceHeatPump, VCHP_IndoorUnitDucted, VCHP_IndoorUnitDuctless or VCHP_IndoorUnitDuctedAndDuctless . For each SC that satisfies this condition, do one of the following:  If SC system in C02 has C11_NotApplicableMessage value, require one row of data; Else If  SC System has C11_IndoorUnitsCount GTE 1 And C04 == CentralGasFurnace require one row of data for each of the quantity of indoor units specified in C11 for that system.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHeating" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="D01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/ Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference to C01, ResidentialSpaceConditioningSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from C02</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D03_IndoorUnitName" type="hvac:IndoorUnitName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Name or  Description of  Area Served</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C11_NotApplicableMessage == N/A then result = N/A  stored in NotApplicableMessage; Else If C11_IndoorUnitsCount = 1 result = C02_ResidentialHvacSystemAreaServed Else user input maximum 15 characters;  As default require value to be unique in this dwelling unit (i.e. unique within the scope of the MCH-01) except for the case when all of the following conditions are true:  D01 == F01 And D02 == F02 And C04_ResidentialHeatingSystemType == CentralGasFurnace.   Allow user to override this uniqueness rule if necessary.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D03_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Name or  Description of  Area Served</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C11_NotApplicableMessage == N/A then result = N/A  stored in NotApplicableMessage; Else If C11_IndoorUnitsCount = 1 result = C02_ResidentialHvacSystemAreaServed Else user input maximum 15 characters;  As default require value to be unique in this dwelling unit (i.e. unique within the scope of the MCH-01) except for the case when all of the following conditions are true:  D01 == F01 And D02 == F02 And C04_ResidentialHeatingSystemType == CentralGasFurnace.   Allow user to override this uniqueness rule if necessary.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="D04_DoesUnitProvideCFI_IAQ" type="comp:DoesUnitProvideCFI_IAQ" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Does Indoor Unit Provide CFI IAQ Ventilation?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any of these conditions are true:  
C11_IndoorUnitsCount  GT 1 Or 
parent CC   VentilationSystemType for this dwelling unit  == Balanced, BalancedERV or BalancedHRV, Or
parent CC  building type == Multifamily
result = false display term No;  
Else If C11_NotApplicableMessage has a value And C04_ResidentialHeatingSystemType == CentralGasFurnace 
user selected Yes (true value) or No (false value); 
 
Else user selected Yes (true value) or No (false value).
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D05_IndoorUnitDucts" type="hvac:IndoorUnitDucts" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Duct Status</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If C06_DuctSystemType == Ductless: result = Ductless; Else If C06_DuctSystemType ==  DuctedSystem  user selects from DuctsGT10Ft display term Ducted [d:gt/] 10ft length and DuctsLTE10Ft display term Ducted [d:lte/] 10ft length; Else If C06_DuctSystemType ==  DuctedAndDuctlessIndoorUnits  user selects from  DuctsGT10Ft display term Ducted [d:gt/] 10ft length, DuctsLTE10Ft display term Ducted [d:lte/] 10ft length and Ductless display term Ductless,</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D06_EfficiencyType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplcableMessage == N/A then result = N/A stored in D06_NotApplicableMessage, Else result = B03_EfficiencyType.</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:EfficiencyType">
                                                <xsd:enumeration value="AFUE"/>
                                                <xsd:enumeration value="COP"/>
                                                <xsd:enumeration value="HSPF"/>
                                                <xsd:enumeration value="HSPF2"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="D06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplcableMessage  == N/A then result = N/A stored in D06_NotApplicableMessage, Else result = B03_EfficiencyType.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D07_EfficiencyInstalledValueAFUE" type="hvac:EfficiencyInstalledValueAFUE" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage   == N/A result = N/A stored in D07_NotApplicableMessage Else User input numeric value  InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display value is PRODUCT (result, 100) to display percent.  Check:  If result GTE B04_EfficiencyMinimumValueAFUE result complies Else flag it as a non-compliant and do not let registration proceed.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D07_EfficiencyInstalledValueCOP" type="hvac:EfficiencyInstalledValueCOP" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage   == N/A result = N/A stored in D07_NotApplicableMessage Else User input numeric value  InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display value is PRODUCT (result, 100) to display percent.  Check:  If result GTE B04_EfficiencyMinimumValueCOP result complies Else flag it as a non-compliant and do not let registration proceed.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D07_EfficiencyInstalledValueHSPF" type="hvac:EfficiencyInstalledValueHSPF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage  == N/A result = N/A stored in D07_NotApplicableMessage Else User input numeric value  InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display value is PRODUCT (result, 100) to display percent; If result GTE B04_EfficiencyMinimumValueHSPF result complies Else flag it as a non-compliant and do not let registration proceed.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D07_EfficiencyHSPF2" type="hvac:EfficiencyHSPF2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage  == N/A result = N/A stored in D07_NotApplicableMessage Else User input numeric value  InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display value is PRODUCT (result, 100) to display percent; If result GTE B04_EfficiencyMinimumValueHSPF2 result complies Else flag it as a non-compliant and do not let registration proceed.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage == N/A result = N/A stored in D04_NotApplicableMessage Else User input numeric value  InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display value is PRODUCT (result, 100) to display percent.   If result GTE B04 EfficiencyMinimumValue  of same type result complies Else flag it as a non-compliant and do not let registration proceed.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="D08_HeatingUnitManufacturer" type="comp:HeatingUnitManufacturer" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Heating Unit Manufacturer</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D09_HeatingUnitModelNumber" type="comp:HeatingUnitModelNumber" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Heating Unit Model Number</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D10_HeatingUnitSerialNumber" type="comp:HeatingUnitSerialNumber" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Heating Unit Serial Number</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D11_HeatingEquipmentRatedCapacity" type="hvac:HeatingEquipmentRatedCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Rated Heating Capacity, Output (Btu/h)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric, xxxx</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="D11a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Cooling System Outdoor Condensing Unit or Package Unit Equipment Information (not heat pumps)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)
 
This Section is applicable only if  one or more systems in section C has  C05_ResidentialCoolingSystemType
  != NoCooling And  != CentralLargePackagedHP, CentralPackagedHP, CentralSplitHP,  DuctlessSplitHP (display term Ductless mini-split HP), DuctedMiniSplitHP, AirToWaterHeatPump, GroundSourceHeatPump, DuctedMultiSplitHP, DuctlessMultiSplitHP,  DuctedDuctlessMultiSplitHP, DuctlessVRF_HP, SmallDuctHighVelocityHP, HydronicHP, HydronicHP_ForcedAir, RoomHP, VCHP_IndoorUnitDucted, VCHP_IndoorUnitDuctless or VCHP_IndoorUnitDuctedAndDuctless;    Require one row of data in this table for each of the SC Systems listed in Section C where C05_ResidentialCoolingSystemType satisfies both of these conditions</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableCooling" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="E01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/ Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference to one of the ResidentialSpaceConditioningSystemName listed in section C</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from C02</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E03_EfficiencyType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Cooling Efficiency Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">As default result = B06_EfficiencyType.  Allow user to override the default and pick from list: EER, EER2, SEER, SEER2, EERSEER, EER2SEER2, CEER;  Flag non-default values and report in project status notes field; a revised parent CC may be required.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:EfficiencyType">
                                             <xsd:enumeration value="CEER"/>
                                             <xsd:enumeration value="EER"/>
                                             <xsd:enumeration value="EER2"/>
                                             <xsd:enumeration value="EER2SEER2"/>
                                             <xsd:enumeration value="EERSEER"/>
                                             <xsd:enumeration value="SEER"/>
                                             <xsd:enumeration value="SEER2"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E04_EfficiencyInstalledValueSEER" type="hvac:EfficiencyInstalledValueSEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value SEER/SEER2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input SEER numeric xx.x  stored in  EfficiencyInstalledValueSEER.   Check value: 
If B07_EfficiencyMinimumValueSEER IsPresent Then
If result  GTE B07_EfficiencyMinimumValueSEER, result complies
Else result does not comply
End If
Else If B07_NotApplicableMessage IsPresent And  If E03_EfficiencyType == SEER or EERSEER   result must be GTE 14 to comply;  
Else result does not comply
End If
Flag non-compliant values and do not let registration proceed if not in compliance.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E04_EfficiencySEER2" type="hvac:EfficiencySEER2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value SEER/SEER2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input SEER2 numeric xx.x  stored in  EfficiencySEER2.   Check value: 
If B07_EfficiencyMinimumValueSEER2 IsPresent Then
If result  GTE B07_EfficiencyMinimumValueSEER2, result complies
Else result does not comply
End If
Else If B07_NotApplicableMessage IsPresent And  If E03_EfficiencyType == SEER2  or EER2SEER2  result must be GTE 13.4 to comply;  
Else result does not comply
End If
Flag non-compliant values and do not let registration proceed if not in compliance.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E04b_EfficiencyInstalledValueEER" type="hvac:EfficiencyInstalledValueEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input EER numeric xx.x  stored in  EfficiencyInstalledValueEER.   Check value: 
If B07b_EfficiencyMinimumValueEER IsPresent Then
If result  GTE B07b_EfficiencyMinimumValueEER, result complies
Else result does not comply
End If
Else If B07b_NotApplicableMessage IsPresent And  If E03_EfficiencyType == EER or EERSEER   result must be GTE 11.7 to comply;  
Else result does not comply
End If
  Flag non-compliant values and do not let registration proceed if not in compliance.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E04b_EfficiencyEER2" type="hvac:EfficiencyEER2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input EER2 numeric xx.x  stored in  EfficiencyEER2.   Check value: 
If B07b_EfficiencyMinimumValueEER2 IsPresent Then
If result  GTE B07b_EfficiencyMinimumValueEER2, result complies
Else result does not comply
End If
Else If B07b_NotApplicableMessage IsPresent And  If E03_EfficiencyType == EER2 or EER2SEER2 result must be GTE 11.2 to comply;  
Else result does not comply
End If
Flag non-compliant values and do not let registration proceed if not in compliance.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E04b_EfficiencyInstalledValueCEER" type="hvac:EfficiencyInstalledValueCEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input CEER numeric xx.x  stored in  EfficiencyInstalledValueCEER.   Check value: If  result  GTE B07_EfficiencyMinimumValueCEER, result complies;  Flag non-compliant values and do not let registration proceed if not in compliance.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E05_CondenserManufacturer" type="comp:CondenserManufacturer" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Manufacturer</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E05_PackageUnitManufacturer" type="comp:PackageUnitManufacturer" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Manufacturer</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E06_CondenserModelNumber" type="comp:CondenserModelNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Model Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E06_PackageUnitModelNumber" type="comp:PackageUnitModelNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Model Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="E07_CondenserSerialNumber" type="comp:CondenserSerialNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Serial Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="E07_PackageUnitSerialNumber" type="comp:PackageUnitSerialNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Serial Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="E08_CoolingEquipmentRatedCapacity" type="hvac:CoolingEquipmentRatedCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Cooling Capacity at Design Conditions (Btu/h)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric xxxxxx</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E09_CondenserNominalCoolingCapacity" type="hvac:CondenserNominalCoolingCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Condenser  Nominal Capacity (tons)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric x.x</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="E09a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Split System Indoor Unit (Coil or Fan Coil) Equipment Information - applicable to DX or hydronic, heating or cooling, coils and fan coil units.</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)
 
This Section is applicable only if one or more systems in section C has C04_ResidentialHeatingSystemType  Or C05_ResidentialCoolingSystemType  == CentralSplitAC, CentralSplitHP, DuctedMultiSplitAC, DuctlessMultiSplitAC, DuctedDuctlessMultiSplitAC, DuctedMultiSplitHP, DuctlessMultiSplitHP, DuctedDuctlessMultiSplitHP, DuctlessSplitAC (display term Ductless mini-split AC), DuctedMiniSplitAC, DuctedMiniSplitHP, AirToWaterHeatPump, GroundSourceHeatPump,  DuctlessSplitHP (display term Ductless mini-split HP), DuctlessVRF_AC, DuctlessVRF_HP, VCHP_IndoorUnitDucted, VCHP_IndoorUnitDuctless, VCHP_IndoorUnitDuctedAndDuctless, EvaporativelyCooledCondenser, GasAbsorptionAC, CombinedHydronicForcedAir, HydronicForcedAir, HydronicHP_ForcedAir, IceStorageAC, SmallDuctHighVelocityHP, SmallDuctHighVelocityAC.   For each SC system in section C with C04 Or C05 == a type in this list, require one row of data for each of the quantity of indoor units in C11_IndoorUnitCount for that system.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Systems with more than one indoor coil or fan coil unit (e.g. multi-split systems) shall provide information for each of the system indoor unit coils or fan coil units.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableCoilEquipment" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="F01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/ Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from C01_ResidentialSpaceConditioningSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from C02</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F03_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or  Description of  Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If C11_IndoorUnitsCount  GTE 1 And all of the following conditions are true:  D01 == F01 And D02 == F02 And C04 == CentralGasFurnace result = D03; Else If C11_IndoorUnitsCount = 1 result = C02_ResidentialHvacSystemAreaServed Else user input maximum 15 characters;  As default require all entries to be unique in this dwelling unit; (i.e. unique within the scope of the MCH-01) except for the case when all of the following conditions are true:   D01 == F01 And D02 == F02 And C04  == CentralGasFurnace.   Allow user to override default  uniqueness rule if necessary.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F04_IndoorUnitType" type="hvac:IndoorUnitType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: CoilAC Display term AC Coil, CoilAndNonfurnaceAH display term Non-furnace Airhandler + Coil, CoilHP display term HP Coil, FancoilAHU display term Fancoil AHU</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F05_IndoorUnitDucts" type="hvac:IndoorUnitDucts" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Duct Status</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If C11_IndoorUnitsCount  GTE 1 And all of the following conditions are true:  D01 == F01 And D02 == F02 And C04 == CentralGasFurnace result = D05; Else If C06_DucSystemType == Ductless: result = Ductless; Else If  C06_DucSystemType == Ducted, user selects from DuctsGT10Ft display term Ducted greater than 10ft length and DuctsLTE10Ft display term Ducted less than or equal to 10ft length; Else If C06_DucSystemType  == DuctedAndDuctlessIndoorUnits  user selects from Ductless, DuctsGT10Ft display term Ducted  greater than 10ft length, DuctsLTE10Ft display term Ducted less than or equal to 10ft length</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F06_DoesUnitProvideCFI_IAQ" type="comp:DoesUnitProvideCFI_IAQ" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Does Indoor Unit Provide CFI IAQ Ventilation?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If C11_IndoorUnitsCount  GTE 1 And all of the following conditions are true:  D01 == F01 And D02 == F02 And C04 == CentralGasFurnace result = D04; 
Else If any of these conditions are true:  C11_IndoorUnitsCount  GT 1 Or parent CC   VentilationSystemType for this dwelling unit  == Balanced Or parent CC building type == Multifamily 
result = false display term No;  
Else user selects Yes (true value) or No (false value)
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="F07_IndoorCoilManufacturer" type="comp:IndoorCoilManufacturer" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Manufacturer</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F07_FanCoilUnitManufacturer" type="comp:FanCoilUnitManufacturer" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Manufacturer</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="F08_IndoorCoilModelNumber" type="comp:IndoorCoilModelNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Model Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F08_FanCoilUnitModelNumber" type="comp:FanCoilUnitModelNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Model Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="F09_IndoorCoilSerialNumber" type="comp:IndoorCoilSerialNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Serial Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F09_FanCoilUnitSerialNumber" type="comp:FanCoilUnitSerialNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Serial Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="F10_IndoorUnitNominalCoolingCapacity" type="hvac:IndoorUnitNominalCoolingCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Nominal Cooling Capacity (tons)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C11_IndoorUnitsCount  GT 1 And  C04_ResidentialHeatingSystemType  Or C05_ResidentialCoolingSystemType == VCHP_IndoorUnitDucted Or VCHP_IndoorUnitDuctedAndDuctless  And F05_IndoorUnitDucts == DuctsGT10Ft Or DuctsLTE10Ft , user input numeric;  Else result = N/A stored in NotApplicableMessage display term Value not required</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F10_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Nominal Cooling Capacity (tons)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C11_IndoorUnitsCount  GT 1 And  C04_ResidentialHeatingSystemType  Or C05_ResidentialCoolingSystemType == VCHP_IndoorUnitDucted Or VCHP_IndoorUnitDuctedAndDuctless  And F05_IndoorUnitDucts == DuctsGT10Ft Or DuctsLTE10Ft , user input numeric;  Else result = N/A stored in NotApplicableMessage display term Value not required</xsd:documentation>
                                             <xsd:appinfo source="displayterm">
                                                <dtyp:displayterm value="N/A">Value not required</dtyp:displayterm>
                                             </xsd:appinfo>
                                             
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="F10a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Heat Pump System - Split System Condensing Unit or Package Unit Equipment Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This Section is applicable only if at least one  system in section C has C04_ResidentialHeatingSystemType Or C05_ResidentialCoolingSystemType == CentralLargePackagedHP, CentralPackagedHP, CentralSplitHP,  DuctlessSplitHP (display term Ductless mini-split HP), DuctedMiniSplitHP, AirToWaterHeatPump, GroundSourceHeatPump,  DuctedMultiSplitHP, DuctlessMultiSplitHP, DuctedDuctlessMultiSplitHP, DuctlessVRF_HP, SmallDuctHighVelocityHP, HydronicHP, HydronicHP_ForcedAir, RoomHP, VCHP_IndoorUnitDucted, VCHP_IndoorUnitDuctless or VCHP_IndoorUnitDuctedAndDuctless.    Require one row of data in this table for each of the SC Systems listed in Section C that has a system in this list.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHeatPump" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/ Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill from C01_ResidentialSpaceConditioningSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill from C02_ResidentialHvacSystemAreaServed for this system</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G03_CondenserManufacturer" type="comp:CondenserManufacturer" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Manufacturer</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G03_PackageUnitManufacturer" type="comp:PackageUnitManufacturer" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Manufacturer</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G04_CondenserModelNumber" type="comp:CondenserModelNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Model Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G04_PackageUnitModelNumber" type="comp:PackageUnitModelNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Model Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G05_CondenserSerialNumber" type="comp:CondenserSerialNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Serial Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G05_PackageUnitSerialNumber" type="comp:PackageUnitSerialNumber" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser or Package Unit Serial Number</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input alphanumeric string max length 50 characters</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="G05a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Heat Pump System - Efficiency and Performance Compliance Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)
 
This Section is applicable only if at least one system in section C has C04_ResidentialHeatingSystemType  Or C05_ResidentialCoolingSystemType == CentralLargePackagedHP, CentralPackagedHP, CentralSplitHP,  DuctlessSplitHP (display term Ductless mini-split HP), DuctedMiniSplitHP, AirToWaterHeatPump, GroundSourceHeatPump,  DuctedMultiSplitHP, DuctlessMultiSplitHP, DuctedDuctlessMultiSplitHP, DuctlessVRF_HP, SmallDuctHighVelocityHP, HydronicHP, HydronicHP_ForcedAir, RoomHP, VCHP_IndoorUnitDucted, VCHP_IndoorUnitDuctless or VCHP_IndoorUnitDuctedAndDuctless.    Require one row of data in this table for each of the SC Systems for which the system type  is in this list.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHeatPumpPerf" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="H01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/ Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill from C01_ResidentialSpaceConditioningSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill from C02_ResidentialHvacSystemAreaServed for this system</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H03_EfficiencyType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplcableMessage == N/A then result = N/A stored in H03_NotApplicableMessage, Else result = B03_EfficiencyType.</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:EfficiencyType">
                                                <xsd:enumeration value="AFUE"/>
                                                <xsd:enumeration value="COP"/>
                                                <xsd:enumeration value="HSPF"/>
                                                <xsd:enumeration value="HSPF2"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="H03_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_NotApplcableMessage == N/A then result = N/A stored in H03_NotApplicableMessage, Else result = B03_EfficiencyType.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H04_EfficiencyInstalledValueAFUE" type="hvac:EfficiencyInstalledValueAFUE" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage == N/A result = N/A stored in H04_NotApplicableMessage 
Else If H03_EfficiencyType == AFUE
User input numeric value  InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display % values as  PRODUCT (result, 100) to display percent.  
End If
Check 
If result GTE B04_EfficiencyMinimumValueAFUE result complies 
Else result does not comply;  Flag non-compliant values and do not let registration proceed.
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H04_EfficiencyInstalledValueCOP" type="hvac:EfficiencyInstalledValueCOP" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage == N/A result = N/A stored in H04_NotApplicableMessage 
Else If H03_EfficiencyType == COP
 User input numeric value   InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display % values as  PRODUCT (result, 100) to display percent.   
End If
Check 
If result GTE B04_EfficiencyMinimumValueCOP result complies Else flag it as a non-compliant and do not let registration proceed
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H04_EfficiencyInstalledValueHSPF" type="hvac:EfficiencyInstalledValueHSPF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage == N/A result = N/A stored in H04_NotApplicableMessage 
Else If H03_EfficiencyType == HSPF 
User input numeric value   InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display % values as  PRODUCT (result, 100) to display percent. 
End If
Check 
If result GTE B04_EfficiencyMinimumValueHSPF result complies Else flag it as a non-compliant and do not let registration proceed
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H04_EfficiencyHSPF2" type="hvac:EfficiencyHSPF2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage == N/A result = N/A stored in H04_NotApplicableMessage 
Else If H03_EfficiencyType == HSPF2 
User input numeric value   InclusiveRange ( 0.00, 1.00); Note stored result is fraction and display % values as  PRODUCT (result, 100) to display percent. 
End If
Check 
If result GTE B04_EfficiencyMinimumValueHSPF2 result complies Else flag it as a non-compliant and do not let registration proceed</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H04_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Heating Efficiency Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B04_NotApplicableMessage == N/A result = N/A stored in H04_NotApplicableMessage 
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="H05_HeatPumpHeatCapacity47F" type="comp:HeatPumpHeatCapacity47F" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Rated Heating Capacity at 47[d:deg/]F  (Btu/h)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric xx.x</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H06_HeatPumpHeatCapacity17F" type="comp:HeatPumpHeatCapacity17F" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Rated Heating Capacity at 17[d:deg/]F  (Btu/h)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric xx.x</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H07_EfficiencyType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Cooling Efficiency Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If B06_EfficiencyType IsPresent As 
default result = B06_EfficiencyType and Allow user to override default and select from list: EER, EER2, SEER, SEER2, EERSEER, EER2SEER2, CEER;  Flag non-default values and report in project status notes field; a revised parent CC may be required.
Else user selects from list:  EER, EER2, SEER, SEER2, EERSEER, EER2SEER2, CEER</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:EfficiencyType">
                                             <xsd:enumeration value="CEER"/>
                                             <xsd:enumeration value="EER"/>
                                             <xsd:enumeration value="EER2"/>
                                             <xsd:enumeration value="EER2SEER2"/>
                                             <xsd:enumeration value="EERSEER"/>
                                             <xsd:enumeration value="SEER"/>
                                             <xsd:enumeration value="SEER2"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H08_EfficiencyInstalledValueSEER" type="hvac:EfficiencyInstalledValueSEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Rated Cooling Efficiency Value SEER/SEER2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input SEER numeric value xx.x within InclusiveRange(0.0, 100.0).   Check result: 
 If B07_NotApplicableMessage IsPresent  
If E03_EfficiencyType == SEER or EERSEER Then 
If result GTE 14 result complies
Else result does not comply
End If
End If
Else If B07_EfficiencyMinimumValueSEER IsPresent
If result  GTE B07_EfficiencyMinimumValueSEER, 
result complies
Else result does not comply
End If
End If
 
Flag non-compliant values and do not let registration proceed if not in compliance.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H08_EfficiencySEER2" type="hvac:EfficiencySEER2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Rated Cooling Efficiency Value SEER/SEER2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input SEER2 numeric xx.x within InclusiveRange(0.0, 100.0).   Check result: 
 If B07_NotApplicableMessage IsPresent   
If E03_EfficiencyType == SEER2 or EER2SEER2 Then 
If result GTE 13.4 result complies
Else result does not comply
End If
End If
Else If B07_EfficiencyMinimumValueSEER2 IsPresent 
If result  GTE B07_EfficiencyMinimumValueSEER2, 
result complies
Else result does not comply
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H08b_EfficiencyInstalledValueEER" type="hvac:EfficiencyInstalledValueEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Rated Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input EER numeric xx.x within InclusiveRange(0.0, 100.0).   Check result: 
 If B07_NotApplicableMessage IsPresent   
If E03_EfficiencyType == EER  or EERSEER Then 
If result GTE 11.7 result complies
Else result does not comply
End If
End If
Else If B07_EfficiencyMinimumValueEER IsPresent 
If result  GTE B07_EfficiencyMinimumValueEER, 
result complies
Else result does not comply
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H08b_EfficiencyEER2" type="hvac:EfficiencyEER2" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Rated Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input EER2 numeric xx.x within InclusiveRange(0.0, 100.0).   Check result: 
 If B07_NotApplicableMessage IsPresent   
If E03_EfficiencyType == EER2 or EER2SEER2 Then 
If result GTE 11.2 result complies
Else result does not comply
End If
End If
Else If B07_EfficiencyMinimumValueEER2 IsPresent 
If result  GTE B07_EfficiencyMinimumValueEER2, 
result complies
Else result does not comply
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H08b_EfficiencyInstalledValueCEER" type="hvac:EfficiencyInstalledValueCEER" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Rated Cooling Efficiency Value EER/EER2/CEER</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input CEER numeric xx.x within InclusiveRange(0.0, 100.0).   Check result: If result  GTE B07_EfficiencyMinimumValueCEER, result complies;  Flag non-compliant values and do not allow registration to proceed if not in compliance.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="H09_CoolingEquipmentRatedCapacity" type="hvac:CoolingEquipmentRatedCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Cooling Capacity at Design Conditions (Btu/h)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric xxxxxx</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H10_CondenserNominalCoolingCapacity" type="hvac:CondenserNominalCoolingCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Condenser Nominal Capacity (ton)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric x.x</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="H10a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_I" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Duct System Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the parent LMCC Certificate of Compliance (CC)
 
This Section is applicable only if  one or more systems in section C has  value for C06_DuctSystemType != Ductless.      Require one row in this table for each space conditioning system in section D field D02 that meets BOTH of the following two conditions: C04_ResidentialHeatingSystemType == GasPackagedFurnace And C06_DuctSystemType == DuctedSystem.  Also require one row in this table for each indoor unit in section D field D03 that meets BOTH of the following two conditions: C04_ResidentialHeatingSystemType == GasPackagedFurnace  And value in D03 != F03.  Also require one row in this table for each space conditioning system in section E field E02 that meets ALL of the following three conditions: C05_ResidentialCoolingSystemType = CentralPackagedAC Or CentralLargePackagedAC And D02 != E02 (the same packaged unit is not already listed in section D) And C06_DuctSystemType == DuctedSystem.  Also require one row in this table for each space conditioning system in Section G field G02 that meets ALL of the following three conditions: C05_ResidentialCoolingSystemType = CentralPackagedHP Or CentralLargePackagedHP And D02 != G02 (the same packaged unit is not already listed in section E) And C06_DuctSystemType == DuctedSystem.  Also require one row of data to be entered for each indoor unit in F03_IndoorUnitName where F05_IndoorUnitDucts == DuctsGT10Ft Or  DuctsLTE10Ft;</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableDuctSystem" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="I01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Identification or Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill from C01_ResidentialSpaceConditioningSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Location or Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto fill from C02_ResidentialHvacSystemAreaServed for this system</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I03_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or  Description of  Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If C04_ResidentialHeatingSystemType ==GasPackagedFurnace result = C02_ResidentialHvacSystemAreaServed; Else if C04_ResidentialHeatingSystemType Or C05_ResidentialCoolingSystemType == CentralPackagedAC, CentralPackagedHP, CentralLargePackagedAC Or CentralLargePackagedHP result = C02_ResidentialHvacSystemAreaServed;   Else reference values in D03_IndoorUnitName and F03_IndoorUnitName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I04_SupplyDuctLocation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Supply Duct Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from C07_DuctLocation as default.  Allow user to overwrite from this list: ConditionedSpace, ConditionedSpaceExcept12ft,  Outdoors, UnconditionedAttic, UnconditionedCrawlSpace, UnconditionedGarage. DuctsInMultipleLocations;  Flag non-default values and report in project status notes field.  a revised parent CC  may be required.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:SupplyDuctLocation">
                                             <xsd:enumeration value="ConditionedSpace"/>
                                             <xsd:enumeration value="ConditionedSpaceExcept12ft"/>
                                             <xsd:enumeration value="DuctsInMultipleLocations"/>
                                             <xsd:enumeration value="Outdoors"/>
                                             <xsd:enumeration value="UnconditionedAttic"/>
                                             <xsd:enumeration value="UnconditionedCrawlSpace"/>
                                             <xsd:enumeration value="UnconditionedGarage"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="I05_DuctRValue" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Supply Duct R-Value</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User picks from: R-0.0, R-2.1, R-4.2, R-6, R-8, R-10, R-12.  Check: If result GTE I14_DuctRValueLimit result complies with following exception, If I08_DuctMinimumRValueExceptions == RValueLT6EntirelyInConditioned then R0.0, R2.1 and R-4.2 comply; Else flag noncompliant value and do not allow registration to proceed.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:DuctRValue">
                                             <xsd:enumeration value="R0.0"/>
                                             <xsd:enumeration value="R10"/>
                                             <xsd:enumeration value="R12"/>
                                             <xsd:enumeration value="R2.1"/>
                                             <xsd:enumeration value="R4.2"/>
                                             <xsd:enumeration value="R6"/>
                                             <xsd:enumeration value="R8"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="I06_ReturnDuctLocation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Return Duct Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from C07_DuctLocation as default.  Allow user to overwrite from this list: ConditionedSpace, ConditionedSpaceExcept12ft,  Outdoors, UnconditionedAttic, UnconditionedCrawlSpace, UnconditionedGarage, DuctsInMultipleLocations;.  Flag non-default values and report in project status notes field.  a revised parent CC  may be required.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:ReturnDuctLocation">
                                             <xsd:enumeration value="ConditionedSpace"/>
                                             <xsd:enumeration value="ConditionedSpaceExcept12ft"/>
                                             <xsd:enumeration value="DuctsInMultipleLocations"/>
                                             <xsd:enumeration value="Outdoors"/>
                                             <xsd:enumeration value="UnconditionedAttic"/>
                                             <xsd:enumeration value="UnconditionedCrawlSpace"/>
                                             <xsd:enumeration value="UnconditionedGarage"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="I07_DuctRValue" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Return Duct R-Value</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User picks from: R-0.0, R-2.1, R-4.2, R-6, R-8, R-10, R-12.  Check: If result GTE I14_DuctRValueLimit result complies with following exception, If I08_DuctMinimumRValueExceptions == RValueLT6EntirelyInConditioned then R0.0, R2.1 and R-4.2 comply;Else flag noncompliant value and do not allow registration to proceed.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:DuctRValue">
                                             <xsd:enumeration value="R0.0"/>
                                             <xsd:enumeration value="R10"/>
                                             <xsd:enumeration value="R12"/>
                                             <xsd:enumeration value="R2.1"/>
                                             <xsd:enumeration value="R4.2"/>
                                             <xsd:enumeration value="R6"/>
                                             <xsd:enumeration value="R8"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="I08_DuctMinimumRValueExceptions" type="comp:DuctMinimumRValueExceptions"  maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Exception from Min R-Value for Ducts In Conditioned Space</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">default result = NoException; Allow user to override and select one or more from list; Allow user to override default and select one or both of the following: 
UninsulatedInDirectConditioning display term Portions of ducts uninsulated  or less than R-6 in conditioned space
 
ALSO 
If I04_SupplyDuctLocation And I06_ReturnDuctLocation == ConditionedSpace 
allow user to select  RValueLT6EntirelyInConditioned   display term Ducts uninsulated or less than R-6 entirely in Conditioned Space
 
End if</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I09_DuctFilterGrilleSizingComplianceMethod" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Method of compliance with Airflow and Fan Efficacy Req's in 160.3(b)5L</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If C05_ResidentialCoolingSystemType == NoCooling, then  result = ExemptNoCooling, display text: Exempt - No Cooling,  
Else If  C05_ResidentialCoolingSystemType == EvaporativeDirect,  EvaporativeIndirect, or EvaporativeIndirectDirect,  then  
result = ExemptEvaporativeSystem, display text :Exempt - Evaporative System, 
 
Else If in Section I fields I11_CanRA3_3AirflowProtocolsTestSystem Or  I12_CanFanEfficacyProtocolsTestSystem == false then 
result = ExemptRA3_3Protocols display text: Exempt - Approved Protocols N/A;    
 
Else If C09_ResidentialCoolingZoneType  == ZoneControlled, Or D04_DoesUnitProvideCFI_IAQ == true Or F04_DoesUnitProvideCFI_IAQ == true  then 
result = HERS_FanEfficacyAirflowRate display text: HERS Verified Fan Efficacy and Airflow Rate, 
 
Else user selects one from the following values:   HERS_FanEfficacyAirflowRate, display text: HERS Verified Fan Efficacy and Airflow Rate;  and HERS_ReturnDuctDesignTable160_3, display text:  HERS verified Return Duct Design per Table 160.3-A, B  
End If</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:DuctFilterGrilleSizingComplianceMethod">
                                             <xsd:enumeration value="ExemptEvaporativeSystem"/>
                                             <xsd:enumeration value="ExemptNoCooling"/>
                                             <xsd:enumeration value="ExemptRA3_3Protocols"/>
                                             <xsd:enumeration value="HERS_FanEfficacyAirflowRate"/>
                                             <xsd:enumeration value="HERS_ReturnDuctDesignTable160_3"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="I10_DuctSystemAirFilterDeviceCount" type="hvac:DuctSystemAirFilterDeviceCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Number of Air Filter Devices on Duct System</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input integer value which determines number or rows per indoor unit system in next section If the total duct length is GT 10 ft</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I11_CanRA3_3AirflowProtocolsTestSystem" type="comp:CanRA3_3AirflowProtocolsTestSystem" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Can Approved Airflow Protocols be used to test this System?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If system type in C04_ResidentialHeatingSystemType Or C05_ResidentialCoolingSystemType == DuctedMultiSplitHP, DuctedDuctlessMultiSplitHP, DuctedMultiSplitAC, DuctedDuctlessMultiSplitAC 
result = false display term No;
 
Else  If system type in C04_ResidentialHeatingSystemType Or C05_ResidentialCoolingSystemType == CentralSplitAC, CentralSplitHP,   CentralPackagedAC, CentralPackagedHP, CentralLargePackagedAC Or CentralLargePackagedHP 
result = true display term Yes  
 
Else user selects from list: Yes (Boolean true display text) or No  (Boolean false display text).  
End if
Check: if result == false, report in project status notes field that exemption from mandatory HERS verification of system airflow has been claimed. Enforcement agency confirmation is recommended.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I12_CanFanEfficacyProtocolsTestSystem" type="comp:CanFanEfficacyProtocolsTestSystem" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Can Approved Fan Efficacy Protocol be used to test this System?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If system type in C04_ResidentialHeatingSystemType Or C05_ResidentialCoolingSystemType == DuctedMultiSplitHP, DuctedDuctlessMultiSplitHP, DuctedMultiSplitAC, DuctedDuctlessMultiSplitAC 
result = CanNotTest display term No;
 
Else If system type in C04_ResidentialHeatingSystemType Or C05_ResidentialCoolingSystemType == CentralSplitAC, CentralSplitHP,   CentralPackagedAC, CentralPackagedHP, CentralLargePackagedAC Or CentralLargePackagedHP 
result = CanTest display term Yes; 
 
Else user selects from list: CanTest display term Yes or CanNotTest display term  No.  
 
Check: if result == CanNotTest report in project status notes field that exemption from mandatory HERS verification of Fan  Efficacy has been claimed. Enforcement agency confirmation is recommended.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I13_IndoorUnitDucts" type="hvac:IndoorUnitDucts" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Duct Length</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If applicable value in D05_IndoorUnitDucts or F05_IndoorUnitDucts == DuctsGT10Ft result = DuctsGT10Ft; Else If applicable value in D05_IndoorUnitDucts or F05_IndoorUnitDucts == DuctsLTE10Ft  result = DuctsLTE10Ft;   Else user selects from list: DuctsLTE10Ft, DuctsGT10Ft</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I14_DuctRValueLimit" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Required New Duct R-Value</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If B10_NotApplicableMessage != N/A reference B10_DuctRValue;  Else If A02_ClimateZone == 3, 5, 6 or 7 result = R6; Else If A02_ClimateZone == 1, 2, 4 or InRange (8,16) result = R8</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:DuctRValueLimit">
                                             <xsd:enumeration value="R6"/>
                                             <xsd:enumeration value="R8"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="I14a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Air Filter Device Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This Section is applicable only if  one or more systems in section C has  value for C06 != Ductless Or If at least one ducted system in section I has I13 == DuctsGT10Ft.  Require one row of data for each Air filter device included in the count I10_DuctSystemAirFilterDeviceCount. for each ducted system in section I has I13 == DuctsGT10Ft.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="J_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Mandatory requirements for air filter devices are specified Section 160.2(b)1.  The installer shall place a sticker in or near each filter grille that displays the  design airflow rate for that filter grille/rack and the maximum allowed clean filter pressure drop at the design airflow rate.  This will inform the occupant of the airflow vs pressure drop performance required for replacement air filters.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableAirFilterDevice" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="J01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Identification or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-fill from C01_ResidentialSpaceConditioningSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Location or Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-fill from C02_ResidentialHvacSystemAreaServed for this system</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J03_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or  Description of  Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from  I03_IndoorUnitName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J04_DuctSystemAirFilterDeviceName" type="hvac:DuctSystemAirFilterDeviceName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Device Name or  Description of  Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">user input text, maximum 20 characters</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J05_DuctSystemAirFilterDeviceType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Rack Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">user select from list:  FilterGrille, Mounted, MountedInline</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:DuctSystemAirFilterDeviceType">
                                             <xsd:enumeration value="FilterGrille"/>
                                             <xsd:enumeration value="Mounted"/>
                                             <xsd:enumeration value="MountedInline"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="J06_AirFilterDeviceAirflowRate" type="hvac:AirFilterDeviceAirflowRate" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Design Airflow Rate for Air Filter Device (cfm)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">user input numeric xxx</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J07_AirFilterDeviceDepth" type="hvac:AirFilterDeviceDepth" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Nominal Depth (inch)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">user input integer GTE 1</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J08_AirFilterDeviceLength" type="hvac:AirFilterDeviceLength" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Nominal Length (inch)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input integer GTE 1.00</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J09_AirFilterDeviceWidth" type="hvac:AirFilterDeviceWidth" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Nominal Width (inch)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input integer GTE 1.00</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J10_AirFilterDeviceFaceArea" type="hvac:AirFilterDeviceFaceArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Calculated Nominal Face Area (inch[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: PRODUCT (J08_AirFilterDeviceLength, J09_AirFilterDeviceWidth )</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="J11_AirFilterDeviceFaceAreaRequiredMin" type="hvac:AirFilterDeviceFaceAreaRequiredMin" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Air Filter Required Minimum Face Area (inch[d:sup]2[/d:sup])</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field; If J07_AirFilterDeviceDepth == 1 result = PRODUCT(J06_AirFilterDeviceAirflowRate / 150, 144) Else result = SpecifiedByDesigner stored in J11_AirFilterDeviceFaceAreaMessage display term Specified by System Designer</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="J11_AirFilterDeviceFaceAreaMessage" type="comp:AirFilterDeviceFaceAreaMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Air Filter Required Minimum Face Area (inch[d:sup]2[/d:sup])</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field; If J07_AirFilterDeviceDepth == 1 result = PRODUCT(J06_AirFilterDeviceAirflowRate / 150, 144) Else result = SpecifiedByDesigner stored in J11_AirFilterDeviceFaceAreaMessage display term Specified by System Designer</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="J12_AirFilterDeviceFaceAreaCompliance" type="comp:AirFilterDeviceFaceAreaCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Face Area  Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: If J11_AirFilterDeviceFaceAreaMessage == SpecifiedByDesigner result = SpecifiedByDesigner Else if J10_AirFilterDeviceFaceArea GTE  J11_AirFilterDeviceFaceAreaRequiredMin result = FaceAreaComplies display term Complies else result = FaceAreaDoesNotComply display term Does Not Comply</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J13_DuctSystemAirFilterDevicePressureDrop" type="hvac:DuctSystemAirFilterDevicePressureDrop" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Design Allowable Pressure Drop for Air Filter Device (inch W.C.)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If J07__AirFilterDeviceDepth == 1  result = 0.1 Else user input numeric x.xx in  InclusiveRange(0.01, 1.5)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="J13a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_K" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Air Filter Device Requirements</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This Section is applicable only if  Section J is applicable</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="KBeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Mandatory Air Filter Device Requirements can be found in Section 160.3(b)1.  Some mandatory requirements may apply in addition to those listed below.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Recirculated and outdoor air filtering</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">All recirculated air and all outdoor air (including make up air) supplied to the occupiable space is filtered before passing through the system's thermal conditioning components.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air Filter Device Requirements</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The space conditioning system shall be designed to accommodate the clean-filter pressure drop imposed by the system air filter device(s). The design airflow rate and maximum allowable clean-filter pressure drop at the design airflow rate applicable to each air filter shall be determined by the system designer.  The system installer shall affix a sticker/label to each system air filter grille/rack locations that discloses the filter's design airflow rate and the filter's maximum allowable clean-filter pressure drop at the design airflow rate.  The sticker/labels shall be permanently affixed to the air filter grille/rack, readily legible, and visible to a person replacing the air filter.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air filter access and regular service</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">All system air filter devices shall be located and installed in such a manner as to allow access and regular service by the system owner.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air filter media efficiency</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The system shall be provided with air filter's having a designated efficiency equal to or greater than MERV 13 when tested in accordance with ASHRAE Standard 52.2, or a particle size efficiency rating equal to or greater than 50% in the 0.30 to1.0 [d:micron/]m range and equal to or greater than 85 percent in the 1.0 - 3.0 [d:micron/]m range when tested in accordance with AHRI Standard 680.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air filter media manufacturer label</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The system shall be provided with air filters that have been labeled by the manufacturer to disclose efficiency and pressure drop ratings that conform to the efficiency and pressure drop requirements for the air filter grilles/racks..</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Prevent air bypassing filter</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Filter racks or grilles shall use gaskets, sealing, or other means to close gaps around inserted filters and prevent air from bypassing the filter.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="KEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_L" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">HERS Verification Requirements for Duct Systems</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if at least one system in section C has C06_DuctSystemType != Ductless.  Require one row or data for each of the indoor units listed in section I I03</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHERSDucts" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="L01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/ Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto filled from Section I  I01_ResidentialSpaceConditioningSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto filled from Section I  I02_ResidentialHvacSystemAreaServed</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L03_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or  Description of  Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference applicable values from section I I03_IndoorUnitName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L04_DuctLeakageTestRequired" type="comp:DuctLeakageTestRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">MCH20   [d:line4/]Duct Leakage Test</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: result = true. display term Yes</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L05_DuctsInConditionedSpaceVerification" type="comp:DuctsInConditionedSpaceVerification" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">MCH21  [d:line3/]Duct  Location Verification</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field:  If I08_DuctMinimumRValueExceptions == RValueLT6EntirelyInConditioned  result = true; 
Else result = false.  
End If
Display Yes and No to represent Boolean result values true and false.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L06_AHU_FanEfficacyVerificationRequired" type="comp:AHU_FanEfficacyVerificationRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">MCH22  [d:line3/]AHU Fan Efficacy (W per cfm)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: If section  I12_CanFanEfficacyProtocolsTestSystem == false result = false; Else If  I09_DuctFilterGrilleSizingComplianceMethod == HERS_FanEfficacyAirflowRate then result = true; Else If C05_ResidentialCoolingSystemType == NoCooling  And D04_DoesUnitProvideCFI_IAQ Or F06_DoesUnitProvideCFI_IAQ == true, then result = true;  Else result = false.  Display Yes and No to represent Boolean result values true and false.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L07_AHU_AirflowRateVerificationRequired" type="comp:AHU_AirflowRateVerificationRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">MCH23  [d:line3/]AHU Airflow Rate (cfm per ton)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: If in Section I field I11_CanRA3_3AirflowProtocolsTestSystem == false result = false; Else If  I09_DuctFilterGrilleSizingComplianceMethod == HERS_FanEfficacyAirflowRate then result = true;  Else If M03_RefrigerantChargeVerificationRequired == true And  L08_ReturnDuctDesignVerificationRequired == false then result = true;   Else If C05_ResidentialCoolingSystemType == NoCooling  And D04_DoesUnitProvideCFI_IAQ Or F06_DoesUnitProvideCFI_IAQ == true, then result = true;  Else result = false.  Display Yes and No to represent Boolean result values true and false.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="L08_ReturnDuctDesignVerificationRequired" type="comp:ReturnDuctDesignVerificationRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">MCH28  [d:line2/]Return Duct Design - Table 160.3-A or B</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field:  If I09_DuctFilterGrilleSizingComplianceMethod ==  HERS_ReturnDuctDesignTable160_3 then result = true, else result = false.  Display Yes and No to represent Boolean result values true and false.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="L08a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_M" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">HERS Verification Requirements For Space Conditioning Equipment</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Require one row of data for each SC System listed in E01_ResidentialSpaceConditioningSystemName and H01_ResidentialSpaceConditioningSystemName</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableHERSEquipment" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="M01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System ID/ Name from parent CC</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from C01_ResidentialSpaceConditioningSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from C02_ResidentialHvacSystemAreaServed</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M03_RefrigerantChargeVerificationRequired" type="comp:RefrigerantChargeVerificationRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">MCH25  [d:line4/]Refrigerant Charge</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field:  If A02_ClimateZone == 2 Or value in InclusiveRange ( 8,15) Then result = true; Else result = false.   Display Yes and No to represent Boolean result values true and false.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="M03a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_N" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Space Conditioning Systems, Ducts and Fans - Mandatory Requirements and Additional Measures</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="N_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Additional mandatory requirements from Section 160.3 that are not listed here may be applicable to some systems.  These requirements may be applicable to only newly installed equipment or portions of the system that are altered.  Existing equipment may be exempt from these requirements.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N00_Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b] Heating Equipment[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Equipment Efficiency</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Equipment Efficiency: All heating equipment must meet the minimum efficiency requirements of Section 110.1 and Section 110.2(a) and the Appliance Efficiency Regulations.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Controls</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Controls:  All unitary heating systems, including heat pumps, must be controlled by a setback thermostat.  These thermostats must be capable of allowing the occupant to program the temperature set points for at least four different periods in 24 hours. See Sections 160.3(a), 110.2(b).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Sizing:</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Sizing:  Heating load calculations must be done on portions of the building served by new heating systems to prevent inadvertent undersizing or oversizing. See sections 160.3(b)1 and 2).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Furnace Temperature Rise</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Furnace Temperature Rise: Central forced-air heating furnace installations must be configured to operate at or below the furnace manufacturer's maximum inlet-to-outlet temperature rise specification. See Section 160.3(b)4.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Standby Losses and Pilot Lights</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Standby Losses and Pilot Lights: Fan-type central furnaces may not have a continuously burning pilot light. Section 110.5 and Section 110.2(d).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N05After_Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b] Cooling Equipment[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Equipment Efficiency:</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Equipment Efficiency:  All cooling equipment must meet the minimum efficiency requirements of Section 110.1 and Section 110.2(a) and the Appliance Efficiency Regulations.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Refrigerant Line Insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Refrigerant Line Insulation:  All refrigerant line insulation in split system air conditioners and heat pumps must meet the R-value and protection requirements of Section 160.3(b)5I1, and Section 160.3(b)6.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Condensing Unit Location</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Condensing Unit Location:  Condensing units shall not be placed within five (5) feet of a dryer vent outlet. See Section 160.3(b)3A.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Liquid Line Filter Drier</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Liquid Line Filter Drier: A liquid line filter drier shall be installed according to the manufacturer's specifications  160.3(b)3B</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Sizing</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Sizing: Cooling load calculations must be done on portions of the building served by new cooling systems to prevent inadvertent undersizing or oversizing. See Section 160.3(b)1 and 2.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N10After_Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b] Air Distribution System Ducts, Plenums and Fans[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N11" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation: The mandatory minimum duct insulation value is R-6 or ducts can be uninsulated if the duct system is located entirely in conditioned space. Note that higher values may be required by the prescriptive or performance requirements. See Section 160.3(b05Aii for exceptions.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N12" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Connections and Closures</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Connections and Closures: All installed air-distribution system ducts and plenums must be, sealed and insulated to meet the requirements of CMC Sections 601.0, 602.0, 603.0, 604.0, 605.0 and ANSI/SMACNA-006-2006.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N12After_Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b] Heat Pump Thermostat[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N13" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Requirements of Section 110.2(b) and (c)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">A thermostat shall be installed that meets the requirements of Section 110.2(b) and Section 110.2(c).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N14" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Manufacturers published installation specifications</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The thermostat shall be installed in accordance with the manufacturers published installation specifications</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N15" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">First stage of heating</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">First stage of heating shall be assigned to heat pump heating.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N16" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Second stage back up heating</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Second stage back up heating shall be set to come on only when the indoor set temperature cannot be met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="NEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="lMCIMCH01cE" type="LMCIMCH01cE">
      <xsd:annotation>
         <xsd:documentation>This element contains all of the data and text required to generate the LMCIMCH01cE compliance report.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
</xsd:schema>
