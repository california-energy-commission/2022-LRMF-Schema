                                       <xsd:element name="D05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exterior Insul. R-Value</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C05_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else user input;  Check: If result  GTE C05 result complies; Else flag as non-compliant and do not allow doc to be registered.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D06_StorageCapacity" type="hvac:StorageCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Storage Volume (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C06_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else user input;  
Check:  If result != C06 flag as non-compliant and do not allow doc to be registered.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Water Heater Storage Volume (gal)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If C06_NotApplicableMessage == N/A result = N/A stored in NotApplicableMessage; Else user input;  
Check:  If result != C06 flag as non-compliant and do not allow doc to be registered.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="D07_TankLocation" type="hvac:TankLocation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from C07 which may be N/A If parent CC is N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="D07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference value from C07 which may be N/A If parent CC  is N/A</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Installed Water Heater Manufacturer Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">Require one row of data for each water heater identified in  Section C</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="TableInstalledWaterHeater" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="E01_WaterHeatingSystemName" type="hvac:WaterHeatingSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Water Heating System ID or Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from B02_WaterHeatingSystemName Or Section B2  Hpi02_WaterHeatingSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E02_WaterHeaterManufacturer" type="hvac:WaterHeaterManufacturer" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Manufacturer</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E03_WaterHeaterModelNumber" type="hvac:WaterHeaterModelNumber" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Model Number</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Mandatory Measures for all Domestic Hot Water Distribution Systems</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Equipment shall meet the applicable requirements of the Appliance Efficiency Regulations (Section 110.3(b)1).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Unfired storage tanks are insulated with an external R-3.5 or combination of R-16 internal and external Insulation. (Section 110.3(c)4).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">All domestic hot water pipes shall be insulated as specified in Section 609.11 of the California Plumbing Code.  [d:l t="*"][d:list] Insulation buried below grade must be installed in a waterproof and non-crushable casing or sleeve. [/d:list][d:list]Piping that penetrates framing members shall not be required to have pipe insulation for the distance of the framing penetration.[/d:list][d:list] Piping that penetrates metal framing shall use grommets, plugs, wrapping or other insulating material to assure that no contact is made with the metal framing. Insulation shall butt securely against all framing members.[/d:list][d:list] Piping installed in interior or exterior walls that is surrounded on all sides by at least 1 inch (2.5 cm) of insulation. [/d:list][d:list] Piping installed in crawl space with a minimum of 1 inch (2.5 cm) of crawl space insulation above and below. [/d:list][d:list]Piping installed in attics with a minimum of 4 inches (10 cm) of attic insulation on top [/d:list][d:list]Piping insulation shall fit tightly, and all elbows and tees shall be fully insulated. [/d:list][/d:l]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">For Gas or Propane Water Heaters: Ensure either a or b are installed (Section 150.0(n)) [d:line1/][d:tab1/]a) A designated space at least 2.5 feet by 2.5 feet and 7 feet tall within 3 feet from the water heater  [d:l t="#"][d:list]A dedicated 120V, 20A electrical receptacle connected to the electrical panel with a 120/240V 3 conductor, 10AWG copper branch circuit, within 3 feet from the water heater and accessible with no obstructions [/d:list][d:list]The conductor shall be labeled with the word "Spare" on both ends; and [/d:list][d:list] A reserved single pole circuit breaker space next to the circuit breaker next to the branch circuit  labeled "Future" 240V shall be provided. [/d:list][d:list]A condensate drain no more than 2 inches higher than the base on water heater for natural draining[/d:list][/d:l]  [d:line1/][d:tab1/]b) A designated space at least 2.5 feet by 2.5 feet and 7 feet tall within 3 feet from the water heater  [d:l t="#"][d:list]A dedicated 240 volt branch circuit shall be installed within 3 feet from the designated space.  The branch circuit shall be rated at 30 amps minimum.  The blank cover shall be identified as "240V ready"; and [/d:list][d:list]The main electrical service panel shall have a reserved space to allow for the installation of a double pole circuit breaker for a future HPWH installation.  The reserved space shall be permanently marked as "For Future 240V use"; and [/d:list][d:list]Either a dedicated cold water supply, or the cold water supply shall pass through the designated HPWH location just before reaching the gas or propane water heater; and [/d:list][d:list] The hot water supply pipe coming out of the gas or propane water heater shall be routed first through the designated HPWH location before serving any fixtures; and [/d:list][d:list]The hot and cold water piping at the designated HPWH location shall be exposed and readily accessible for future installation of a HPWH; and [/d:list][d:list]A condensate drain no more than 2 inches higher than the base of the installed water heater, and allows natural draining without pump assistance. [/d:list][/d:l]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Compact Hot Water Distribution (CHWDS) RA4.4.6</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if at least one dwelling has B10_CompactDistributionType == BasicType Or at least one dwelling has Section B2 Hpi_08_CompactDistributionType == BasicType.  Require one row of data, reporting the longest distances, for each dwelling unit  with B10_CompactDistributionType == BasicType Ot Hpi_08_CompactDistributionType == BasicType.
 
In this section parent CC refers to the parent LMCC</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">For dwelling units with multiple systems, enter the master bath distance and kitchen distance to the closest water heater, and enter the average of the furthest fixture to each water heater. .</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableCompact" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_ResidentialDwellingUnitName" type="com:ResidentialDwellingUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Dwelling Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from A01_ResidentialDwellingUnitName Or Section A2 Hpd01_ResidentialDwellingUnitName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G02_TotalStoryCount" type="com:TotalStoryCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Number of Stories</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If prescriptive path, user select from list:  1,2 or 3; Else result = N/A stored in NotApplicableMessage;</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G02_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Number of Stories</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If prescriptive path, user select from list:  1,2 or 3; Else result = N/A stored in NotApplicableMessage;</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="G03_WaterHeaterToBath" type="comp:WaterHeaterToBath" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Master Bath distance of furthest fixture to Water Heater in feet</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance parent CC  result = parent CC  value; Else If prescriptive parent CC  or not available in XML,  user input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G04_WaterHeaterToKitchen" type="comp:WaterHeaterToKitchen" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Kitchen distance of furthest fixture to Water Heater in feet</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance parent CC  result = parent CC  value; Else If prescriptive parent CC or not available in XML,  user input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G05_WaterHeaterToFurthest3rdFixture" type="comp:WaterHeaterToFurthest3rdFixture" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Furthest Third furthest fixture to Water Heater in feet (Avg for multiple water heaters)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance parent CC  result = parent CC  value; Else If prescriptive parent CC or not available in XML,  user input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G06_WaterHeaterWeightedDistance" type="comp:WaterHeaterWeightedDistance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Weighted Distance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance parent CC  result = parent CC  value; Else If prescriptive parent CC or not available in XML, Then
If G01_ResidentialDwellingUnitName is loaded from A01_ResidentialDwellingUnitName Then
If A09_ResidentialDHWDwellingUnitDistributionType == StandardDistribution 
result =  (G03_WaterHeaterToBath x  0.4)  +  (G04_WaterHeaterToKitchen x 0.4)  + (G05_WaterHeaterToFurthest3rdFixture x 0.2); 
 
Else If A09_ResidentialDHWDwellingUnitDistributionType == DemandRecirculationManualControl result = G05_WaterHeaterToFurthest3rdFixture
End If
 
Else If G01_ResidentialDwellingUnitName is loaded from Hpd01_ResidentialDwellingUnitName Then
If Hpd07_ResidentialDHWDwellingUnitDistributionType == StandardDistribution 
result =  (G03_WaterHeaterToBath x  0.4)  +  (G04_WaterHeaterToKitchen x 0.4)  + (G05_WaterHeaterToFurthest3rdFixture x 0.2); 
 
Else If Hpd07_ResidentialDHWDwellingUnitDistributionType == DemandRecirculationManualControl result = G05_WaterHeaterToFurthest3rdFixture
End If
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G07_WaterHeaterQualificationDistance" type="comp:WaterHeaterQualificationDistance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qualification Distance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If performance parent CC  result = parent CC  value; Else If prescriptive parent CC or not available in XML,  
If G01_ResidentialDwellingUnitName is loaded from A01_ResidentialDwellingUnitName Then
result =  ((CompactDistCoefA + CompactDistCoefB x CFA) / A05_WaterHeaterTotalSystemCount); 
Else If G01_ResidentialDwellingUnitName is loaded from Hpd01_ResidentialDwellingUnitName Then
result =  ((CompactDistCoefA + CompactDistCoefB x CFA) / Hpd04_WaterHeaterTotalSystemCount);
End If 
Values for CompactDistCoefA,CompactDistCoefB are found in the Table  4.4.6-2 Coefficients for the Qualification Distance Calculation following section G.  Value for CFA (conditioned floor area)  comes from parent CF1R.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G08_CompactFactor" type="comp:CompactFactor" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Compactness Factor</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance parent CC  result = parent CC  value; and store in CompactFactor
Else If prescriptive parent CC
result = NotApplicable stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Compactness Factor</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If performance parent CC  result = parent CC  value; and store in CompactFactor
Else If prescriptive parent CC
result = NotApplicable stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G09_CompactFactorCalculated" type="comp:CompactFactorCalculated" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Calculated Compactness Factor</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field:
result = 0.3 + 0.4 x (G06_WaterHeaterWeightedDistance / G07_WaterHeaterQualificationDistance)
 
If performance parent CC result must be LTE G08_CompactFactor
Else If prescriptive parent CC result must be LTE 0.7
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Calculated Compactness Factor</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="GEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Parallel Piping Requirements (PP) (RA4.4.4)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has A09_ResidentialDHWDwellingUnitDistributionType == StandardDistributionCentralParallelPiping Or at least one water heating system that has Section A2  Hpd07_ResidentialDHWDwellingUnitDistributionType == StandardDistributionCentralParallelPiping</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="H_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize this distribution type shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Each central manifold has 15 feet or less of pipe between manifold and water heater</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">For manifolds that include valves, the manifold must be readily accessible in accordance with the plumbing code. (RA 4.4.4)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Hot water distribution system piping from the manifold to the fixtures and appliances must take the most direct path.  For instance, piping from a second story manifold cannot supply the first floor.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The hot water distribution piping must be separated by at least 2 inches from any other hot water supply piping, and at least 6 inches from any cold water supply piping.  Alternatively, the hot water supply piping must be insulated to the thicknesses shown in TABLE 120.3-A.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_I" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Point of Use Requirements (POU) (RA4.4.5)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has A09_  ResidentialDHWDwellingUnitDistributionType == PointOfUse Or at least one water heating system that has  Section A2  Hpd07_ResidentialDHWDwellingUnitDistributionType == PointOfUse</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="I_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize this distribution type shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">All hot water supply pipe run lengths are equal to or less than the maximum values shown below, based on the pipe diameter.   If a combination of piping is used in a single run then one half the allowed length of each size is the maximum installed length.  [d:line1/]The maximum allowed length of piping for the longest run terminating in:  [d:line1/][d:tab1/]3/8 inch - For only one pipe size - max length allowed is 15 feet.   [d:line1/][d:tab2/]For combination pipe sizes the max allowed length of 3/8 inch piping is 7.5 feet, of 1/2 inch piping is 5 feet, and 3/4 inch piping is 2.5 feet.   [d:line1/][d:tab1/]1/2 inch - For only one pipe size - max length allowed is 10 feet. [d:line1/][d:tab2/]For combination pipe sizes the allowed length of 1/2 inch piping is 5 feet, and 3/4  inch piping is 2.5 feet.  [d:line1/][d:tab1/]3/4 inch  - For only one pipe size = 5 feet</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="IEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Mandatory Requirements For All Recirculation Systems (RA4.4.7)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has A09_ResidentialDHWDwellingUnitDistributionType == RecirculationNonDemand, DemandRecirculationManualControl Or DemandRecirculationSensorControls Or at least one water heating system that has  Section A2  Hpd07_ResidentialDHWDwellingUnitDistributionType == RecirculationNonDemand, DemandRecirculationManualControl Or DemandRecirculationSensorControls</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="J_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize a recirculation system shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">A check valve is located between the recirculation pump and the water heater to prevent unintentional recirculation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Piping must take the most direct path between water heater and fixtures.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Insulation is not required on the cold water line when it is used as the return.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">If more than one loop installed, each loop shall have its own pump and controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="JEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_K" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Recirculation Non-Demand Controls Requirements (R-ND) (RA4.4.8)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has A09_ResidentialDHWDwellingUnitDistributionType == RecirculationNonDemand Or at least one water heating system that has  Section A2  Hpd07_ResidentialDHWDwellingUnitDistributionType == RecirculationNonDemand</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="K_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize this distribution type shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The active control shall be either: timer, temperature, or time and temperature. Timers shall be set to less than 24 hours. The temperature sensor shall be connected to the piping and to the controls for the pump.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="KEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_L" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Demand Recirculation Manual Control (R-DRmc) (RA4.4.9)/ Sensor Control Requirements (RDRsc) (RA4.4.10)</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if there is at least one water heating system that has A09_ResidentialDHWDwellingUnitDistributionType == DemandRecirculationManualControl Or DemandRecirculationSensorControls Or at least one water heating system that has  Section A2  Hpd07_ResidentialDHWDwellingUnitDistributionType == DemandRecirculationManualControl Or DemandRecirculationSensorControls</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="L_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Systems that utilize either of these distribution types shall comply with these requirements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The system operates "on-demand", meaning that the pump begins to operate shortly before or immediately after hot water draw begins, and stops when the return water temperature reaches a certain threshold value. For Demand Recirculation Manual Control, the pump shall be turned on using a manual switch system.  For Demand Recirculation Sensor Control, the pump shall be turned on using a sensor system.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The controls shall be located in the kitchen, bathroom, and any hot water fixture location that is at least 20 feet from the water heater.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Manual controls may be activated by wired or wireless mechanisms.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Sensor controls may be activated by wired or wireless mechanisms, including buttons, motion sensors, door switches and flow switches.  Each control shall have standby power of 1 Watt or less.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Pump and control placement shall meet one of the following criteria:  [d:l t="*"][d:list]When a dedicated return line has been installed the pump, controls and thermo-sensor are installed at the end of the supply portion of the recirculation loop; or  [/d:list][d:list]The pump and controls are installed on the dedicated return line near the water heater, and the thermo-sensor is installed in an accessible location as close to the end of the supply portion of the recirculation loop as possible, or  [/d:list][d:list]When the cold water line is used as the return, the pump, demand controls and thermo-sensor shall be installed in an accessible location at the end of supply portion of the hot water distribution line (typically under a sink).[/d:list][/d:l]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">After the pump has been activated, the controls shall allow the pump to operate until the water temperature at the thermo-sensor rises to one of the following values: (RA4.4.13)  [d:l t="*"][d:list] Not more than 10[d:deg/]F ( 5.6[d:deg/]C ) above the initial temperature of the water in the pipe  [/d:list][d:list]Not more than 102[d:deg/]F (38.9[d:deg/]C).[/d:list][/d:l]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Controls shall limit operation to no more than 5 minutes following activation.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The responsible person's signature on this compliance document affirms that all applicable requirements in this table have been met.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="lMCIPLB02E" type="LMCIPLB02E">
      <xsd:annotation>
         <xsd:documentation>This element contains all of the data and text required to generate the LMCIPLB02E compliance report.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
</xsd:schema>
