                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpi07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Location</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from Hpd07 which may be NA</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpi08_LoopTankCapacity" type="hvac:LoopTankCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Volume</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input; If Hpd08_LoopTankCapacity IsPresent, result must be GTE to Hpd08_LoopTankCapacity</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpi08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Volume</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from Hpd08_NotApplicableMessage if present</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpi09_LoopTankTotalInsulation" type="hvac:LoopTankTotalInsulation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Total Insulation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input; If Hpd09_LoopTankTotalInsulation IsPresent result must be GTE to Hpd09_LoopTankTotalInsulation</xsd:documentation>
                                          </xsd:annotation>
