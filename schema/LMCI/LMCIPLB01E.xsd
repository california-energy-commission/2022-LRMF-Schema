                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpi10_PipeInsulationThickness" type="hvac:PipeInsulationThickness" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Pipe Insulation Thickness</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input; If Hpd10_PipeInsulationThickness IsPresent, result must be GTE to Hpd10_PipeInsulationThickness</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpi10_NotApplicableMessage" type="comp:NotApplicableMessage" >
