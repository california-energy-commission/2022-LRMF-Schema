                                    <xsd:element name="Hpi04_TankLocation" type="hvac:TankLocation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Primary Tank Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference values from Hpd04_TankLocation</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpi05_StorageCapacity" type="hvac:StorageCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Tank Volume</xsd:documentation>
