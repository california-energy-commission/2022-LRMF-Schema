                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Tank Total Insulation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input; If Hpd06_PrimaryTankTotalInsulation IsPresent, result must be GTE to Hpd06_PrimaryTankTotalInsulation</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hpi06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Primary Tank Total Insulation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Reference values from Hpd06_NotApplicableMessage if present</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hpi07_TankLocation" type="hvac:TankLocation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Loop Tank Location</xsd:documentation>
