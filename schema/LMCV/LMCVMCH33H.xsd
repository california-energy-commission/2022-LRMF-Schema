<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/LMCVMCH33H"
   xmlns="http://www.lmonte.com/besm/LMCVMCH33H"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2022.0.000">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/ResCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/ResBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/ResEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/ResLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/ResHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/ResCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTag" fixed="LMCVMCH33H"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentType" fixed="CERTIFICATE OF VERIFICATION"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleRes" fixed="Variable Capacity Heat Pump Compliance Credit"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderHERS" />
                     <xsd:element ref="lMCVMCH33H" />
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonCF3R" />
                     <xsd:element name="Footer" type="comp:Footer" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTag" fixed="LMCVMCH33H"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="LMCVMCH33H">
      <xsd:annotation>
         <xsd:documentation>Variable Capacity Heat Pump Compliance Credit</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">VCHP System Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the LMCC Certificate of Compliance (CC) and MCH-01 refers to the LMCI-MCH-01 and MCH-25 refers to the LMCI-MCH-25</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Procedures for verification of VCHP compliance credit eligibility are described in the Energy Code Reference Appendices Section RA3.4.4.3.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ResidentialSpaceConditioningSystemName" type="hvac:ResidentialSpaceConditioningSystemName" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">SC System ID/Name from parent CC</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">auto filled text: referenced from MCH01</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ResidentialHvacSystemAreaServed" type="hvac:ResidentialHvacSystemAreaServed" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">SC System Description of Area Served</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">auto filled text: referenced from MCH01</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_ResidentialHvacSystemConditionedArea" type="hvac:ResidentialHvacSystemConditionedArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Floor area Served by System (ft[d:sup]2[/d:sup])</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">auto filled text: referenced from MCH01a field D03, Mch01d field D03</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_DoesMCH25VerifyRefrigCharge" type="comp:DoesMCH25VerifyRefrigCharge" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Status: Refrigerant charge verification from MCH-25</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If the system has a registered MCH-25 that indicates compliance with the refrigerant charge verification requirement, then result = true display term System Complies with Refrigerant Charge verification Requirements; Else result = false display term System does not comply.  A registered MCH-25 for this sytem is required</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_DoesIndoorUnitSupplyAllRooms" type="comp:DoesIndoorUnitSupplyAllRooms" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Verification: Is conditioned airflow supplied to all habitable rooms in accordance with the procedure in RA3.1.4.1.7?</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from list  true result display term Indoor unit complies according to the criteria in RA3.1.4.1.7, and false result display term Indoor unit does not comply</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">VCHP Indoor Unit Information</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section parent CC refers to the LMCC Certificate of Compliance (CC) and MCH-01 refers to the LMCI-MCH-01 and MCH-23 refers to the LMCI-MCH-23
 
Provide one row of data for each of the indoor units listed on the MCH-01 that are associated with the SC system ID and area served data in A01 and A02.  For indoor unit information refer to MCH-01a Section G or J; otherwise refer to MCH-01d Section H, K, L</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Ducted indoor units are required to be certified to the Energy Commission as low static systems, and included in the list of certified indoor units published on the Energy Commission website at the following URL:  https://www.energy.ca.gov/rules-and-regulations/building-energy-efficiency/manufacturer-certification-building-equipment.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableIndoorUnit" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="B01_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from MCH01 as follows: G03 on MCH01a and H03 on MCH01d for split systems, J03 on MCH01a and K03 or L03 on MCH01d for packaged systems</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B02_IndoorUnitType" type="hvac:IndoorUnitType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Installed Indoor Unit Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If MCH01 has value as follows: G04 on MCH01a and H04 on MCH01d auto-fill from MCH01; Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B02_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Installed Indoor Unit Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If MCH01 has value as follows: G04 on MCH01a and H04 on MCH01d auto-fill from MCH01; Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B03_IndoorUnitDucts" type="hvac:IndoorUnitDucts" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Duct Status</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If MCH01 has value as follows: G05 on MCH01a and H05 on MCH01d auto-fill from MCH01; Else result = N/A stored in NotApplicableMessage.  Allowed values are: DuctsGT10Ft display term Ducted greater than 10ft length, DuctsLTE10Ft display term Ducted less than or equal to 10ft length and Ductless display term No ducts</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B03_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit duct Status</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If MCH01 has value as follows: G05 on MCH01a and H05 on MCH01d auto-fill from MCH01; Else result = N/A stored in NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="B04_IndoorUnitAreaServed" type="hvac:IndoorUnitAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Conditioned Floor Area served By The Indoor Unit (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Reference value from MCH-33 as default but let User override the default and enter a different numeric value xxxx.x.
Require that the sum of the values in this column are equal to A03_ResidentialHvacSystemAreaServed as condition for completing this report.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B05_DuctSystemAirFilterDeviceCount" type="hvac:DuctSystemAirFilterDeviceCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Number of Air Filter Devices on Indoor Unit</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDucts == Ductless result = N/A stored in NotApplicableMessage;  Else Auto-fill from MCH01as follows:J11 on MCH01a and L12 on MCH01d</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Number of Air Filter Devices on Indoor Unit</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDucts == Ductless result = N/A stored in NotApplicableMessage;  Else Auto-fill from MCH01as follows:J11 on MCH01a and L12 on MCH01d</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B06_AirflowRequiredMinimumTarget" type="hvac:AirflowRequiredMinimumTarget" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Required Minimum System Airflow Rate (cfm)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDuct == Ductless  result = N/A stored in NotApplicablemessage; Else auto-filled from MCH23 D02 for all variants</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B06_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Indoor Unit Required Minimum System Airflow Rate (cfm)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDuct == Ductless  result = N/A stored in NotApplicablemessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B07_DoesMCH23VerifyAirFlowRate" type="comp:DoesMCH23VerifyAirFlowRate" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Status: Airflow Rate Verification from MCH-23</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDuct == Ductless  result = N/A stored in NotApplicablemessage; Else If system has registered MCH-23 that indicates compliance with minimum airflow rate requirement, result = true display term  System complies with airflow rate verification requirements; Else result = false display term System does not comply.  A registered MCH-23 for this system is required</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B07_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Status: Airflow Rate Verification from MCH-23</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDuct == Ductless  result = N/A stored in NotApplicablemessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B08_VerifyFanNonContinuousOperation" type="comp:VerifyFanNonContinuousOperation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Is Field Verification of Default Non-Continuous Fan Operation Required?</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDuct == Ductless  result = N/A stored in NotApplicablemessage; Else If parent CC indicates credit for [Certified Auto-Fan] operation was specified result = true display term Yes; Else result = false display term No</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B08_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Is Field Verification of Default Non-Continuous Fan Operation Required?</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDuct == Ductless  result = N/A stored in NotApplicablemessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="B09_IsUnitCertifiedLowStatic" type="comp:IsUnitCertifiedLowStatic" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Verification: Is Ducted Low Static Indoor Unit Certified to CEC?</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDuct == Ductless  result = N/A stored in NotApplicablemessage; Else user selects from list:  true result display term Complies - indoor unit is certified low-static in the CEC listings and false result display term Does not comply - indoor unit is not certified</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="B09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Verification: Is Ducted Low Static Indoor Unit Certified to CEC?</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B03_IndoorUnitDuct == Ductless  result = N/A stored in NotApplicablemessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="B09a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Verification: Ducted Indoor Units Located Entirely in Directly Conditioned Space - RA3.1.4.3.8</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If in section B at least one indoor unit has B03 != Ductless; Require one row for each indoor unit in B01 that satisfies this condition</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Ducted indoor units shall be verified in accordance with the Verified Low Leakage Ducts in Conditioned Space procedure in Section RA3.1.4.3.8</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDucted" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="C01_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C02_AreDuctsEntirelyInConditionedSpace" type="comp:AreDuctsEntirelyInConditionedSpace" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">A Visual Inspection Shall Confirm the Space Conditioning Distribution Location (RA3.1.4.1.3)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: true result display term Entirely in conditioned space, false result display term Not entirely in conditioned space</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="false">Not entirely in conditioned space</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C03_DuctSystemLeakageActual" type="comp:DuctSystemLeakageActual" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Measured Duct Leakage to Outside (cfm) Using RA3.1.4.3.4</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="C04_DuctedEntirelyConditionedCompliance" type="comp:DuctedEntirelyConditionedCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compliance Statement</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If C03_DuctSystemLeakageActual LTE 25 cfm And C02_AreDuctsEntirelyInConditionedSpace == true result = true display term Complies; Else result = false display term Does Not Comply</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="C04a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Verification: Ductless Indoor Units Located Entirely in Directly Conditioned Space - RA3.1.4.1.8</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If in section B at least one indoor unit has B03 == Ductless; Require one row for each indoor unit in B01 that satisfies this condition</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">A visual inspection shall confirm that ductless indoor units are located entirely in conditioned space in accordance with the procedures of RA3.1.4.1.8.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDuctless" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="D01_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D02_DuctlessIndoorUnitLocation" type="hvac:DuctlessIndoorUnitLocation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Installation Location Verification</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: MountedInside display term Indoor unit mounted entirely on the surface of walls, ceilings, or floors, 
 PartiallyInUnconditioned display term Indoor unit protrudes through the air barrier into unconditioned spaces, PartiallyInIndirectlyConditioned display term Visually confirmed: Indoor unit protrudes into directly conditioned spaces wholly inside both the thermal boundary and the air barrier of the dwelling, PartiallyInIndirectlyConditionedInstaller display term Not Visually confirmed: Installer certifies the  Indoor unit protrudes into directly conditioned spaces wholly inside both the thermal boundary and the air barrier of the dwelling</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D03_DuctlessEntirelyConditionedCompliance" type="comp:DuctlessEntirelyConditionedCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compliance Statement</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If D02_DuctlessIndoorUnitLocation == MountedInside Or PartiallyInIndirectlyConditioned  result = DuctlessUnitComplies display term Complies; Else If D02_DuctlessIndoorUnitLocation == PartiallyInUnconditionedElse result = DuctlessUnitDoesNotComply display term Does Not Comply; Else If D02_DuctlessIndoorUnitLocation == PartiallyInIndirectlyConditionedInstaller result = DuctlessUnitCompliesNoSampling</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="D03a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Verification: Wall Mounted Thermostats - RA3.4.5</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If in section B at least one indoor unit has B04_IndoorUnitAreaServed GTE 150; Require one row for each indoor unit in B01 that satisfies this condition</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Field verification according to the procedure in RA3.4.5 shall confirm that VCHP space conditioning zones that are greater than 150 ft[d:sup]2[/d:sup], are controlled by a permanently installed wall-mounted thermostat.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableThermostats" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="E01_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E02_ThermostatInZoneServedByUnit" type="comp:ThermostatInZoneServedByUnit" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Is a Wall-mounted Thermostat Installed in the Zone Served by the Indoor Unit?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: true result display term Yes, false result display term No</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E03_ThermostatControlsZoneUnit" type="comp:ThermostatControlsZoneUnit" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Does the Thermostat Control the Zone's Indoor Unit?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: true result display term Yes, false result display term No</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E04_ThermostatPermanentlyMounted" type="comp:ThermostatPermanentlyMounted" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Is the Thermostat Mounted Permanently to the Wall?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: true result display term Yes, false result display term No</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="E05_IndoorUnitThermostatCompliance" type="comp:IndoorUnitThermostatCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compliance Statement</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If E02_ThermostatInZoneServedByUnit And E03_ThermostatControlsZoneUnit And E04_ThermostatPermanentlyMounted == true result = true display term Complies, Else result = false display term Does not comply</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="E05a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Verification: Non-Continuous Fan Operation  - RA3.4.6</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If in section B at least one indoor unit has B08_VerifyFanNonContinuousOperation == true; Require one row for each indoor unit in B01 that satisfies this condition</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">If the certificate of compliance indicates non-continuous indoor unit fan operation was specified for compliance credit, then the system shall be field verified in accordance with the procedures in RA3.4.6 to confirm that the installed system's indoor unit + outdoor unit combination does not operate the fan continuously when the system thermostat is not calling for conditioning.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableFan" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="F01_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from B01</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F02_IsFanCertifiedNonContinuous" type="comp:IsFanCertifiedNonContinuous" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">In Non-Continuous Default Fan Operation Shown in CEC Certification Listings?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: true result display term Indoor unit + oudoor unit combination is certified non-continuous in the CEC listings, false result display term Not certified</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F03_HeatingFanOperatesOnlyOnCall" type="comp:HeatingFanOperatesOnlyOnCall" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Does Indoor Unit Air Distribution Fan Operate When There is No Call For Heating?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: true result display term Fan does not operate between calls for heating; false result display term Fan operates continuously</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F04_CoolingFanOperatesOnlyOnCall" type="comp:CoolingFanOperatesOnlyOnCall" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Does Indoor Unit Air Distribution Fan Operate When There is No Call For Cooling?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: true result display term Fan does not operate between calls for cooling; false result display term Fan operates continuously</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F05_NonContinuousFanCompliance" type="comp:NonContinuousFanCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compliance Statement</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If F02_IsFanCertifiedNonContinuous And F03_HeatingFanOperatesOnlyOnCall And F04_CoolingFanOperatesOnlyOnCall == true result = true display term Complies; Else result = false display term Does not comply</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="F05a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">Verification: Installed Air Filter Sizing and Pressure Drop - RA3.1.4.7 and RA3.1.4.8</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">In this section MCH01 refers to the LMCI-MCH-01
 
The section is required if in section B at least one indoor unit has B03 == DuctsGT10Ft; Require one row of data for each in the quantity reported in B05_DuctSystemAirFilterDeviceCount for each indoor unit in B01 that satisfies this requirement</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Begin Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Nominal 2-inch or greater depth air filters shall be sized by the system designer to accommodate a maximum allowable clean-filter pressure drop of 0.1 inch W.C. at the air filter's design airflow rate as verified according to the procedures in RA3.1.4.8.  Nominal one-inch minimum depth air filters shall be allowed if the filter face area is sized based on a maximum face velocity of 150 ft per minute at the air filter design airflow rate according to the procedures in RA3.1.4.7.  In order to inform the occupant of the airflow and clean filter pressure drop performance required for replacement of air filters, the installer shall place a sticker in or near the filter grille displaying the air filter design airflow rate and maximum allowed clean filter pressure drop at the design airflow rate as required by Standards Section 160.2(b)1Biv.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableAirFilter" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_IndoorUnitName" type="hvac:IndoorUnitName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Indoor Unit Name or Description of Area Served</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from parent MCH01 as follows: K03 from MCH01a or M03 from MCH01d</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G02_DuctSystemAirFilterDeviceName" type="hvac:DuctSystemAirFilterDeviceName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Name or Description of Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from parent MCH01 as follows: K04 from MCH01a or M04 from MCH01d</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03_DuctSystemAirFilterDeviceType" type="hvac:DuctSystemAirFilterDeviceType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Device type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from parent MCH01 as follows: K05 from MCH01a or M05 from MCH01d</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G04_AirFilterDeviceAirflowRate" type="hvac:AirFilterDeviceAirflowRate" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Design Airflow Rate for Air Filter Device (cfm)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If B05_DuctSystemAirFilterDeviceCount == 1 then 
result = B06_AirflowRequiredMinimumTarget; 
 
Else If B05_DuctSystemAirFilterDeviceCount GT 1 reference value from parent MCH01 as follows: K06 from MCH01a or M06 from MCH01d
End If
 
Check results:  Require sum of all G04_AirFilterDeviceAirflowRate values to be greater than or equal to B06_AirflowRequiredMinimumTarget  as a condition of completion of this document.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G05_AirFilterDeviceDepth" type="hvac:AirFilterDeviceDepth" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Nominal Depth (inch)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enter numeric</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G06_AirFilterDeviceLength" type="hvac:AirFilterDeviceLength" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Nominal Length (inch)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enter numeric</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G07_AirFilterDeviceWidth" type="hvac:AirFilterDeviceWidth" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Nominal Width (inch)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enter numeric</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G08_AirFilterDeviceFaceArea" type="hvac:AirFilterDeviceFaceArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Calculated Nominal Face Area (inch[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: result = G06_AirFilterDeviceLength * G07_AirFilterDeviceWidth</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G09_AirFilterDeviceFaceAreaRequiredMin" type="hvac:AirFilterDeviceFaceAreaRequiredMin" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Air Filter Required Minimum Face Area (inch[d:sup]2[/d:sup])</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G05_AirFilterDeviceDepth == 1 then result = (G04_AirFilterDeviceAirflowRate / 150) * 144; Else result = SpecifiedByDesigner stored in AirFilterDeviceFaceAreaMessage display term Specified by System Designer</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G09_AirFilterDeviceFaceAreaMessage" type="comp:AirFilterDeviceFaceAreaMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Air Filter Required Minimum Face Area (inch[d:sup]2[/d:sup])</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G05_AirFilterDeviceDepth == 1 then result = (G04_AirFilterDeviceAirflowRate / 150) * 144; Else result = SpecifiedByDesigner stored in AirFilterDeviceFaceAreaMessage display term Specified by System Designer</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="G10_AirFilterDeviceFaceAreaCompliance" type="comp:AirFilterDeviceFaceAreaCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Face Area Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If G09_AirFilterDeviceFaceAreaMessage  == SpecifiedByDesigner then result = SpecifiedByDesigner; Else If G08_AirFilterDeviceFaceArea  GTE G09_AirFilterDeviceFaceAreaRequiredMin   result = FaceAreaComplies; Else result = FaceAreaDoesNotComply;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G11_DuctSystemAirFilterDevicePressureDrop" type="hvac:DuctSystemAirFilterDevicePressureDrop" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter Rated Pressure Drop at Design Airflow Rate (inch W.C)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input numeric value in range GTE 0.01 to LTE1.5</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G12_AirFilterPressureDropCompliance" type="comp:AirFilterPressureDropCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Air Filter  Pressure Drop Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If G11_DuctSystemAirFilterDevicePressureDrop LTE 0.1 result = true display term Complies; Else result = false display term Does not comply</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="G12a_SectionComments" type="comp:SectionComments"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Notes:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">VCHP System Compliance Statement</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="H01_ComplianceVCHP" type="comp:ComplianceVCHP" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance Statement</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">For all fields that are present, if all these fields  == true: A04_DoesMCH25VerifyRefrigCharge,  
 A05_DoesIndoorUnitSupplyAllRooms, B07_DoesMCH23VerifyAirFlowRate, B09_IsUnitCertifiedLowStatic, C04_DuctedEntirelyConditionedComply,  E05_IndoorUnitThermostatCompliance, And G12_AirFilterPressureDropCompliance And If D03_DuctlessEntirelyConditionedComply != DuctlessUnitDoesNotComply And If G10_AirFilterDeviceFaceAreaCompliance != FaceAreaDoesNotComply result = true display term VCHP System Complies; Else result = false display term VCHP System Does Not Comply</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="lMCVMCH33H" type="LMCVMCH33H">
      <xsd:annotation>
         <xsd:documentation>This element contains all of the data and text required to generate the LMCVMCH33H compliance report.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
</xsd:schema>
