                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Installed EER/EER2</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">user input - EER/EER2 listed in the Directory used to certify product performance 
Note If Mch01 C07_EfficiencyMinimumValueEER IsPresent and assigned to E01_EfficiencyMinimumValueEER user must enter EER value
Else If Mch01 C07_EfficiencyMinimumValueEER2 IsPresent and assigned to E01_EfficiencyMinimumValueEER user must enter EER2 value</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E03_DoesCoolingEquipmentComplyWithEER" type="comp:DoesCoolingEquipmentComplyWithEER" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance Statement:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field: If E02_EfficiencyInstalledValueEER GTE E01_EfficiencyMinimumValueEER tresult  = true.   display term System Passes EER/EER2 Verification   Else result = false display term System Fails.  Do not proceed with document</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="TRUE">System Passes EER/EER2 Verification</dtyp:displayterm>
                        </xsd:appinfo>
                        
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="EEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">End Note 1</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Signature by responsible person on this compliance document certifies that the installed cooling equipment meets or exceeds the required value listed on the parent MCH-26 Certificate of Installation.[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
